              "is_end": false,
              "node_type": "normal",
              "children": {
                "3": {
                  "id": "3",
                  "speaker": "Pandirna",
                  "text": "I'm not. Shaking.",
                  "context": "NodeContext: Bitten off, quietly insistent. She's close to snapping.",
                  "checkflags": [],
