      "node_type": "normal",
      "children": {}
    },
    "3": {
      "id": "3",
      "speaker": "Marcoryl",
      "text": "That poor child. This should never have happened. You people should have left when Kagha asked.",
      "context": "",
      "checkflags": [
        "DEN_DruidServant_HasMet_002 = False",
        "DEN_ShadowDruid_State_KidDied",
        "TIEFLING"
      ],
      "setflags": [
