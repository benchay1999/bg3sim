    "2": {
      "id": "2",
      "speaker": "Marcoryl",
      "text": "Goblin attacks and now a dead child. We never should have opened our gates.",
      "context": "",
      "checkflags": [
        "DEN_DruidServant_HasMet_002 = False",
        "DEN_ShadowDruid_State_KidDied"
