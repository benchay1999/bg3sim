                "6": {
                  "id": "6",
                  "speaker": "Player",
                  "text": "Halsin is dead. I thought you should know.",
                  "context": "",
                  "checkflags": [
                    "DEN_ShadowDruidLeader_State_KaghaKnowsHalsinDead = False"
