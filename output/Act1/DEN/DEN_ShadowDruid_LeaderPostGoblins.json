      "children": {
        "11": {
          "id": "11",
          "speaker": "Kagha",
          "text": "The last prayer will be spoken and the rite will conclude. When the coming army swarms, the brambles will keep us safe.",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "30": {
              "id": "30",
              "speaker": "Kagha",
              "text": "I suppose you want thanks. I'm sure the tieflings have plenty.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "2": {
                  "id": "2",
                  "speaker": "Player",
                  "text": "I've discovered the truth. You're allied with Shadow Druids.",
                  "context": "",
                  "checkflags": [
                    "DEN_ShadowDruid_Knows_RecruitmentLetter",
                    "DEN_ShadowDruid_Event_KaghaTurnedGood = False",
                    "e223fd36-c236-f2d3-d812-25ed88e2702a_ConfrontedKagha = False"
