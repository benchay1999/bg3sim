    "15": {
      "id": "15",
      "speaker": "Kagha",
      "text": "Finally, we'll be rid of the outlanders' infernal stench.",
      "context": "",
      "checkflags": [
        "GLO_Halsin_Knows_IsFound = False",
        "DEN_ShadowDruid_Event_KaghaTurnedGood = False"
