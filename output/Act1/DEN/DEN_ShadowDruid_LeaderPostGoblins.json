                  "node_type": "normal",
                  "children": {
                    "33": {
                      "id": "33",
                      "speaker": "Kagha",
                      "text": "Halsin, gone.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "27": {
                          "id": "27",
                          "speaker": "Kagha",
                          "text": "He turned his back on the circle to stumble after the past. How fitting that it's consumed him.",
                          "context": "",
                          "checkflags": [],
                          "setflags": [],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
                          "goto": "",
                          "link": "",
                          "is_end": false,
                          "node_type": "normal",
                          "children": {
                            "4": {
                              "id": "4",
                              "speaker": "Kagha",
                              "text": "Well. A broodmother looks forward, never back. I am First Druid now. And I will keep my children safe.",
                              "context": "",
                              "checkflags": [],
                              "setflags": [],
                              "ruletags": [],
                              "approval": [],
                              "rolls": "",
                              "goto": "",
                              "link": "",
