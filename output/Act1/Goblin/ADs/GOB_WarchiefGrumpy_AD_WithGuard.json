      "children": {
        "11": {
          "id": "11",
          "speaker": "Lookout Grush",
          "text": "Ah, as good as, boss! Got a professional workin' on him right now.",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "8": {
              "id": "8",
              "speaker": "Zurga",
              "text": "That a fact? Hope so, for your sake - plenty of room next to the piggy if you're lying.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
