        "2": {
          "id": "2",
          "speaker": "Player",
          "text": "Sounds like a bloody scrap. Where did you see that?",
          "context": "",
          "checkflags": [
            "GOB_Checkpoint_State_Backup002HasMet = False",
            "BALDURIAN = False"
          ],
          "setflags": [],
          "ruletags": [],
