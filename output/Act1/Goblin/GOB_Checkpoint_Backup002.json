          "rolls": "",
          "goto": "",
          "link": "1",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "1": {
              "id": "1",
              "speaker": "Devout Wasp",
              "text": "You ain't hear? We sacked Waukeen's Rest and left it to burn.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "7": {
                  "id": "7",
                  "speaker": "Devout Wasp",
                  "text": "Drow flingin' magic, gnolls snappin' off heads. Inn's nothin' but a pile of ash by now.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "4": {
                      "id": "4",
                      "speaker": "Devout Wasp",
                      "text": "Glorious.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [
