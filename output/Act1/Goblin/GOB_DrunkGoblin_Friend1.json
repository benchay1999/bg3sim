            "20": {
              "id": "20",
              "speaker": "Player",
              "text": "I... kind of enjoyed myself.",
              "context": "NodeContext: She really wants to kiss his feet, like you did, but she's afraid to ask.",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "14": {
                  "id": "14",
                  "speaker": "Tracker Kirz",
                  "text": "Oh, I... I knew it. I just 'ave to try. Just gather my courage and try.",
                  "context": "NodeContext: She really wants to kiss his feet, like you did, but she's afraid to ask.",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": true,
                  "node_type": "normal",
                  "children": {}
