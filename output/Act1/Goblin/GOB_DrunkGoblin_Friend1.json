      "children": {
        "9": {
          "id": "9",
          "speaker": "Tracker Kirz",
          "text": "Don't go thinkin' you could beat 'im in a fair fight, you hear me? Not Crusher...",
          "context": "NodeContext: Displeased. You beat her hero.",
