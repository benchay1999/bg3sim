              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "24": {
                  "id": "24",
                  "speaker": "Tracker Kirz",
                  "text": "Yes, yes that's... what you'd think, innit?",
                  "context": "NodeContext: She actually doesn't think so, but she's ashamed to admit it. She also expected you to give her a different answer.",
