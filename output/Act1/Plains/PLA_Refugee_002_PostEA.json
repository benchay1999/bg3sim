              "is_end": false,
              "node_type": "normal",
              "children": {
                "14": {
                  "id": "14",
                  "speaker": "Trynn",
                  "text": "There's a locked door down the hatch. Doesn't look like anyone managed to get in.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "6": {
                      "id": "6",
                      "speaker": "Trynn",
                      "text": "If there's any gold left, that's where it'll be.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
