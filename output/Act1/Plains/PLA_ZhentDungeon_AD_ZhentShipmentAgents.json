                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "1": {
                      "id": "1",
                      "speaker": "Rugan",
                      "text": "Avoiding the Underdark, for one.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
