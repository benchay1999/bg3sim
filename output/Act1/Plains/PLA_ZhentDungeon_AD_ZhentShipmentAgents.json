      "is_end": false,
      "node_type": "normal",
      "children": {
        "6": {
          "id": "6",
          "speaker": "Rugan",
          "text": "It's pronounced 'Zent', lad. The H is silent.",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "10": {
              "id": "10",
              "speaker": "Olly",
              "text": "I don't care. I want out of this damn cave.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "14": {
                  "id": "14",
                  "speaker": "Zarys",
                  "text": "Why, scared of gahosts?",
