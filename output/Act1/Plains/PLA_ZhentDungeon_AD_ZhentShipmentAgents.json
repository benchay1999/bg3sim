      "is_end": false,
      "node_type": "normal",
      "children": {
        "8": {
          "id": "8",
          "speaker": "Zarys",
          "text": "Not close enough.",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "0": {
              "id": "0",
              "speaker": "Rugan",
              "text": "Lass, if you hate me that much, hire someone to kill me.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "4": {
                  "id": "4",
                  "speaker": "Zarys",
                  "text": "My way's cheaper.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": true,
                  "node_type": "normal",
                  "children": {}
                }
              }
            }
          }
        }
      }
    },
    "7": {
      "id": "7",
      "speaker": "Rugan",
