              }
            },
            "8": {
              "id": "8",
              "speaker": "Player",
              "text": "Gods, that's horrific. I'll have your head!",
              "context": "NodeContext: Drugh = DROOG. Like GOOD, not FOOD. Expletive = 'fucks'",
              "checkflags": [
                "UND_DuergarLoyalPatroller_Event_SnappedSpine",
