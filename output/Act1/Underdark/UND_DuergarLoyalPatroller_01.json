              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "3": {
                  "id": "3",
                  "speaker": "Murmath",
                  "text": "Go ahead - try me, ya shit!",
                  "context": "",
                  "checkflags": [],
