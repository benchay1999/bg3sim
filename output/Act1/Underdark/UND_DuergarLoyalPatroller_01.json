              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "12": {
                  "id": "12",
                  "speaker": "Murmath",
                  "text": "Me - who else? My spiders sussed 'em out of hiding, then Nere taught 'em a thing or two, and I let 'em fly.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
