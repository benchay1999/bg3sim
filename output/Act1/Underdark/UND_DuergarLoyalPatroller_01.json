            "17": {
              "id": "17",
              "speaker": "Player",
              "text": "Who hung these drow out to dry?",
              "context": "NodeContext: Drugh = DROOG. Like GOOD, not FOOD. Expletive = 'fucks'",
              "checkflags": [],
              "setflags": [
                "UND_DuergarLoyalPatroller_Event_AskedAboutDrow"
