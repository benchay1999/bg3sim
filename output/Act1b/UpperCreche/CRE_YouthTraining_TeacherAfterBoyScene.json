              "children": {
                "81": {
                  "id": "81",
                  "speaker": "Sa'varsh Kethk",
                  "text": "A ridiculous notion. No student of mine would be permitted to indulge such heresies.",
                  "context": "NodeContext: Edge of threat",
                  "checkflags": [],
