    "58": {
      "id": "58",
      "speaker": "Sa'varsh Kethk",
      "text": "What is it? These insufferable interruptions are distracting from my lessons.",
      "context": "NodeContext: Almost threatening",
      "checkflags": [
        "CRE_YouthTraining_HasMet_Teacher = False",
        "CRE_YouthTraining_State_BoyDead = False"
      ],
      "setflags": [
