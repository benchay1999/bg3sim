                  "children": {
                    "73": {
                      "id": "73",
                      "speaker": "Sa'varsh Kethk",
                      "text": "The name pollutes the very air expelled in its uttering.",
                      "context": "NodeContext: Edge of threat",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
