              "is_end": false,
              "node_type": "normal",
              "children": {
                "2": {
                  "id": "2",
                  "speaker": "Player",
                  "text": "Did you see the Duke?",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "12",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "12": {
                      "id": "12",
                      "speaker": "Gauntlet Yeva",
                      "text": "Yeah. He was in one of the pods, but the cultists took him away.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "9": {
                          "id": "9",
                          "speaker": "Gauntlet Yeva",
                          "text": "He was still human. And still trying to put up a fight.",
                          "context": "",
                          "checkflags": [],
                          "setflags": [],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
                          "goto": "",
                          "link": "",
