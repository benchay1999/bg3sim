      "is_end": false,
      "node_type": "normal",
      "children": {
        "7": {
          "id": "7",
          "speaker": "Gauntlet Yeva",
          "text": "This is a bleeding shitshow. Half my squad's dead, the other half's sporting tentacles.",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "12",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "1": {
              "id": "1",
              "speaker": "Gauntlet Yeva",
              "text": "We came here to rescue Duke Ravengard, but we didn't stand a chance.",
              "context": "",
              "checkflags": [],
