                      "link": "8",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "31": {
                          "id": "31",
                          "speaker": "Isobel",
                          "text": "You deserve to know that much - and more.",
                          "context": "",
                          "checkflags": [],
                          "setflags": [],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
                          "goto": "",
                          "link": "8",
                          "is_end": false,
                          "node_type": "normal",
                          "children": {}
                        }
                      }
                    },
                    "12": {
                      "id": "12",
                      "speaker": "Player",
                      "text": "I'd like that.",
