                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "16": {
                          "id": "16",
                          "speaker": "Isobel",
                          "text": "To be perfectly frank, I'm exhausted. It feels a bit as though I've been dropped into another dimension.",
                          "context": "",
                          "checkflags": [],
                          "setflags": [],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
