          "children": {
            "27": {
              "id": "27",
              "speaker": "Isobel",
              "text": "Oh, I am. It's a momentous day. I suppose I need to catch my breath or the momentum will outpace me.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "5",
