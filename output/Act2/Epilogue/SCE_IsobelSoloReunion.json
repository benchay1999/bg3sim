                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "13": {
                          "id": "13",
                          "speaker": "Isobel",
                          "text": "As would I. I'll see you later. Looking forward to it.",
                          "context": "",
                          "checkflags": [],
                          "setflags": [
                            "SCE_IsobelNightsongReunion_JoiningCamp"
