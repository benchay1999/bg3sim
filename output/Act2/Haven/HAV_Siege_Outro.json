                        "40": {
                          "id": "40",
                          "speaker": "Player",
                          "text": "I am the damn light. Nothing can extinguish me.",
                          "context": "",
                          "checkflags": [],
                          "setflags": [],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
                          "goto": "",
