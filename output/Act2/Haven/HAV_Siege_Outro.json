            "50": {
              "id": "50",
              "speaker": "Jaheira",
              "text": "I led them here - to this.",
              "context": "CinematicNodeContext: (Cine) On \u2018to this' she waves her arm signalling the carnage all around.",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
