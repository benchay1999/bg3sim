            "4": {
              "id": "4",
              "speaker": "Zealot Zira",
              "text": "If you think I'm walking all the way to Baldur's Gate without a litter, you don't know me at all.",
              "context": "NodeContext: Scornful - she's used to having slaves",
              "checkflags": [],
              "setflags": [],
