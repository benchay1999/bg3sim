  "dialogue": {
    "0": {
      "id": "0",
      "speaker": "Zealot Soreni",
      "text": "You ought to be kinder, you know. To the prisoners, I mean.",
      "context": "NodeContext: Advice, absent observation",
      "checkflags": [],
      "setflags": [
        "72f2c4d8-2c3a-dabb-8c0b-993a1b018dbd_A"
