    },
    "5": {
      "id": "5",
      "speaker": "Zealot Bennel",
      "text": "Nothing to report, Dis-",
      "context": "NodeContext: starting to say 'Disciple' but cut off",
      "checkflags": [
        "e6aef01a-0468-5046-e519-b9b624c704dc_B"
