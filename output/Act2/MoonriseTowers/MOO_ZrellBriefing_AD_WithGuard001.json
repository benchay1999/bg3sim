      "approval": [],
      "rolls": "",
      "goto": "",
      "link": "",
      "is_end": false,
      "node_type": "normal",
      "children": {
        "9": {
          "id": "9",
          "speaker": "Disciple Z'rell",
          "text": "Then why are you reporting, soldier? Interrupt me again, and you will serve Disciple Balthazar instead. Do you want that?",
          "context": "NodeContext: Cutting off underling in a rage - the soldier interrupted her",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "3": {
              "id": "3",
              "speaker": "Zealot Bennel",
              "text": "No, Disciple. Forgiveness, Disciple.",
              "context": "NodeContext: Shook, put in their place.",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": true,
