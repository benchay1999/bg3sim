            "49": {
              "id": "49",
              "speaker": "Player",
              "text": "I don't usually bargain with rats. If I let you live, what's in it for me?",
              "context": "NodeContext: Speaking of memories",
              "checkflags": [
                "SHA_LastJusticiar_RatsNegotiate_Event_RatOfferedTreasure = False",
                "SHA_LastJusticiar_State_PlayerTruce = False"
