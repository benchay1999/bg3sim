              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "8",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "3": {
                  "id": "3",
                  "speaker": "",
                  "text": "There are treasures. Useless to us now, but once they had value - meaning. We hid them away, untouched for a lifetime.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "61": {
                      "id": "61",
                      "speaker": "",
                      "text": "Keep your word, and we will reveal them to you.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {}
                    }
                  }
                }
              }
            },
            "18": {
              "id": "18",
              "speaker": "Player",
              "text": "All right. Your lives for this treasure. You'd better be level with me.",
              "context": "NodeContext: Speaking of memories",
              "checkflags": [
