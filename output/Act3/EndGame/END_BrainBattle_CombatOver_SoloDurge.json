                                  "is_end": false,
                                  "node_type": "visualstate",
                                  "children": {
                                    "27": {
                                      "id": "27",
                                      "speaker": "Narrator",
                                      "text": "*And just like that, it's over. Whatever's left of you is subsumed by the overwhelming strength of your desires.*",
                                      "context": "",
                                      "checkflags": [],
                                      "setflags": [],
                                      "ruletags": [],
                                      "approval": [],
                                      "rolls": "",
                                      "goto": "",
                                      "link": "16",
                                      "is_end": false,
                                      "node_type": "normal",
                                      "children": {}
                                    }
                                  }
                                }
                              }
                            },
                            "19": {
                              "id": "19",
                              "speaker": "Player",
                              "text": "Whatever it is, you deserve it.",
