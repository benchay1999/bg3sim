        "21": {
          "id": "21",
          "speaker": "Narrator",
          "text": "*If only you'd defied your Father when you had the chance, and freed yourself of your dark desires... but it's too late now.*",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "4",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "11": {
              "id": "11",
              "speaker": "Narrator",
              "text": "*Your destiny is set. Your very blood shall clot with the Urge, and that hungry desire will consume you.*",
              "context": "",
              "checkflags": [
                "ORI_DarkUrge_State_BhaalDisappointed"
              ],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "4",
              "is_end": false,
              "node_type": "normal",
              "children": {}
            },
            "2": {
              "id": "2",
              "speaker": "Narrator",
              "text": "*You disobeyed the Lord of Murder. But his hold on you remains... It gnaws... You are hungry.*",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "16",
