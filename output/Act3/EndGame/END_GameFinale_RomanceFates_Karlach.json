                                  "node_type": "normal",
                                  "children": {}
                                },
                                "24": {
                                  "id": "24",
                                  "speaker": "Player",
                                  "text": "I have to admit, I'll miss the inferno.",
                                  "context": "",
                                  "checkflags": [],
                                  "setflags": [],
                                  "ruletags": [],
                                  "approval": [],
                                  "rolls": "",
                                  "goto": "",
                                  "link": "42",
