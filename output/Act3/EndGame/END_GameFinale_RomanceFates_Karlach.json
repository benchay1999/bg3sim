                                  "setflags": [],
                                  "ruletags": [],
                                  "approval": [],
                                  "rolls": "",
                                  "goto": "",
                                  "link": "30",
                                  "is_end": false,
                                  "node_type": "normal",
                                  "children": {
                                    "1": {
                                      "id": "1",
                                      "speaker": "Karlach",
                                      "text": "Somewhere quiet. Somewhere private. Somewhere we can ask a million questions and contemplate their answers.",
                                      "context": "",
                                      "checkflags": [],
                                      "setflags": [],
                                      "ruletags": [],
                                      "approval": [],
                                      "rolls": "",
                                      "goto": "",
                                      "link": "30",
                                      "is_end": false,
                                      "node_type": "normal",
                                      "children": {
                                        "42": {
                                          "id": "42",
                                          "speaker": "Karlach",
                                          "text": "This is only the beginning, my love. We will reach heights of which our former selves could not conceive.",
                                          "context": "",
                                          "checkflags": [],
                                          "setflags": [],
                                          "ruletags": [],
                                          "approval": [],
                                          "rolls": "",
                                          "goto": "",
                                          "link": "30",
                                          "is_end": false,
                                          "node_type": "normal",
                                          "children": {}
                                        }
