                                "46": {
                                  "id": "46",
                                  "speaker": "Player",
                                  "text": "We accomplished so much together. We are a formidable team.",
                                  "context": "",
                                  "checkflags": [],
                                  "setflags": [],
                                  "ruletags": [],
                                  "approval": [],
                                  "rolls": "",
                                  "goto": "",
                                  "link": "42",
