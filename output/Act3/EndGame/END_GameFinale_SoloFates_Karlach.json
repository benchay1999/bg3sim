              "goto": "",
              "link": "1",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "0": {
                  "id": "0",
                  "speaker": "Karlach",
                  "text": "That's over, I'm afraid. What comes next is... the end.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "1",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "9": {
                      "id": "9",
                      "speaker": "Karlach",
                      "text": "Can I do this?",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "1",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "24": {
                          "id": "24",
                          "speaker": "Karlach",
                          "text": "Yes.",
                          "context": "",
                          "checkflags": [],
                          "setflags": [
                            "END_GameFinale_Event_KarlachChoseDeath"
                          ],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
                          "goto": "",
                          "link": "1",
                          "is_end": false,
                          "node_type": "normal",
                          "children": {
                            "18": {
                              "id": "18",
                              "speaker": "Karlach",
                              "text": "Yes. Yes, I can. Yes. All I need is to see the sky one last time.",
                              "context": "",
                              "checkflags": [],
                              "setflags": [],
                              "ruletags": [],
                              "approval": [],
                              "rolls": "",
                              "goto": "",
                              "link": "",
                              "is_end": true,
                              "node_type": "normal",
                              "children": {}
                            }
                          }
                        },
                        "23": {
                          "id": "23",
                          "speaker": "Karlach",
                          "text": "No.",
                          "context": "",
                          "checkflags": [],
                          "setflags": [],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
                          "goto": "",
                          "link": "1",
                          "is_end": false,
                          "node_type": "normal",
                          "children": {
                            "34": {
                              "id": "34",
                              "speaker": "Karlach",
                              "text": "I don't think I have any other choice. Unless I go back to the Hells.",
                              "context": "",
                              "checkflags": [],
                              "setflags": [],
                              "ruletags": [],
                              "approval": [],
                              "rolls": "",
                              "goto": "",
                              "link": "1",
