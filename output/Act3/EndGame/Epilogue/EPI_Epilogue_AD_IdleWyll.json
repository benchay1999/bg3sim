      "is_end": false,
      "node_type": "normal",
      "children": {
        "11": {
          "id": "11",
          "speaker": "Wyll",
          "text": "I don't know how to explain it. Maybe I don't need to.",
          "context": "NodeContext: With wonder",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
