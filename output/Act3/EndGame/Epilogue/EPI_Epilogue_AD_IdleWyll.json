    "1": {
      "id": "1",
      "speaker": "Wyll",
      "text": "A night without care - no worry, no obligation. It's been too long.",
      "context": "NodeContext: With wonder",
      "checkflags": [
        "GLO_Wyll_State_GrandDuke",
        "73be3269-32bb-0c7a-bfa6-762cfe04b6dd_A"
