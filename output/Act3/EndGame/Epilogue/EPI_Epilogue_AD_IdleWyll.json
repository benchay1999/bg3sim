      "children": {}
    },
    "2": {
      "id": "2",
      "speaker": "Wyll",
      "text": "All my favourite people in one place. Not to mention, all my favourite wines.",
      "context": "NodeContext: With wonder",
      "checkflags": [
        "73be3269-32bb-0c7a-bfa6-762cfe04b6dd_A"
      ],
      "setflags": [
