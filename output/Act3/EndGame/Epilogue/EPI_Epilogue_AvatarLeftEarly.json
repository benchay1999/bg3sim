                  "children": {
                    "47": {
                      "id": "47",
                      "speaker": "Withers",
                      "text": "The transition from life to death undoes many a mortal's mind. Thou art confused, but thou willst collect thy thoughts, in time.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "0",
