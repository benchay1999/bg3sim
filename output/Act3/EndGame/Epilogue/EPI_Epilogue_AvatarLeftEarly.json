                "64": {
                  "id": "64",
                  "speaker": "Player",
                  "text": "Of course. I reached for greatness. If I failed, so be it - at least I tried.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "0",
                  "is_end": false,
