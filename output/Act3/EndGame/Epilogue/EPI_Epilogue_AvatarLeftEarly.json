              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "47",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "35": {
                  "id": "35",
                  "speaker": "Player",
                  "text": "Wait - I'm dead? I don't understand? This can't be real...",
                  "context": "",
