      "is_end": false,
      "node_type": "normal",
      "children": {
        "25": {
          "id": "25",
          "speaker": "Narrator",
          "text": "*To speak is not enough. You wish to explore the brain of your friend with your beak.*",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "38",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "33": {
              "id": "33",
              "speaker": "Narrator",
              "text": "*You consider Karlach's brain stem - the stalk meant to regulate her body's temperature. Will it come pre-cooked?*",
              "context": "",
              "checkflags": [
                "REALLY_KARLACH"
              ],
              "setflags": [],
              "ruletags": [],
              "approval": [],
