  "dialogue": {
    "3": {
      "id": "3",
      "speaker": "Narrator",
      "text": "*All this stimulating conversation leaves you hungry.*",
      "context": "",
      "checkflags": [],
      "setflags": [
        "EPI_Epilogue_State_MindFlayerUrgeOccurred"
