                                  "node_type": "normal",
                                  "children": {
                                    "20": {
                                      "id": "20",
                                      "speaker": "Raphael",
                                      "text": "||",
                                      "context": "",
                                      "checkflags": [],
                                      "setflags": [],
                                      "ruletags": [],
                                      "approval": [],
                                      "rolls": "",
                                      "goto": "",
                                      "link": "",
                                      "is_end": true,
                                      "node_type": "normal",
                                      "children": {}
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "17": {
      "id": "17",
      "speaker": "Raphael",
      "text": "|Well well, isn't it my favorite knight in shining armor.|",
      "context": "",
      "checkflags": [
        "ORI_Gale_Event_GaveCrownToMystra = False",
        "ORI_Gale_State_IsGod = False"
      ],
      "setflags": [],
