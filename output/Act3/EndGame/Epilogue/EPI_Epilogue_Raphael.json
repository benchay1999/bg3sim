                                  "rolls": "",
                                  "goto": "",
                                  "link": "",
                                  "is_end": false,
                                  "node_type": "taganswer",
                                  "children": {}
                                }
                              }
                            },
                            "8": {
                              "id": "8",
                              "speaker": "Player",
                              "text": "|Gale is the one to speak to.|",
                              "context": "",
                              "checkflags": [],
                              "setflags": [],
                              "ruletags": [],
                              "approval": [],
                              "rolls": "",
                              "goto": "",
                              "link": "",
                              "is_end": false,
                              "node_type": "normal",
                              "children": {
                                "14": {
                                  "id": "14",
                                  "speaker": "Raphael",
                                  "text": "|Gale is not the one I'm speaking to now, am I!|",
                                  "context": "",
                                  "checkflags": [],
                                  "setflags": [],
                                  "ruletags": [],
                                  "approval": [],
                                  "rolls": "",
                                  "goto": "",
                                  "link": "",
