  "dialogue": {
    "0": {
      "id": "0",
      "speaker": "Unholy Assassin Mitchia",
      "text": "You are not fit to bear Bhaal's blood. Get out.",
      "context": "NodeContext: Vicious, threatening",
      "checkflags": [
        "ORI_DarkUrge_State_BhaalResisted",
        "DARK_URGE"
