                  "children": {
                    "4": {
                      "id": "4",
                      "speaker": "",
                      "text": "This is the Chamber of Egress. The portals here criss-cross the planes, but they won't open to the likes of you and me.",
                      "context": "NodeContext: enthusiastic",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
