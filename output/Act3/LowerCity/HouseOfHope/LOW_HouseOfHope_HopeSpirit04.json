                                  "children": {
                                    "3": {
                                      "id": "3",
                                      "speaker": "",
                                      "text": "HE LOVES YOU HE LOVES YOU NOT HE LOVES YOU HE LOVES YOU NOT",
                                      "context": "NodeContext: using a childish sing-song to disguise anxiety",
                                      "checkflags": [],
                                      "setflags": [],
