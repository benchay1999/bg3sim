                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "1": {
                      "id": "1",
                      "speaker": "Phrennog",
                      "text": "I am, thank you.",
                      "context": "NodeContext: Slightly annoyed",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
