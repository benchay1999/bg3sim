          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "5": {
              "id": "5",
              "speaker": "Caitlin",
              "text": "No. You're telling me the Baldur's Mouth kept information back?",
