  "dialogue": {
    "0": {
      "id": "0",
      "speaker": "Ciara Blank",
      "text": "Things aren't like they used to be - broadsheet says it all, really.",
      "context": "",
      "checkflags": [],
      "setflags": [
        "LOW_BaldursMouth_HasMet_Wanderer_06"
      ],
