    "60": {
      "id": "60",
      "speaker": "Bosun Gannet",
      "text": "It's a sad day in the 'Maid. A sad and bloody day.",
      "context": "NodeContext: sad, heartbroken",
      "checkflags": [
        "LOW_BlushingMermaid_State_QuenoraGrizly_PermaDefeated",
        "LOW_BlushingMermaid_State_HagIsTrulyDead"
      ],
      "setflags": [],
      "ruletags": [],
      "approval": [],
      "rolls": "",
      "goto": "",
      "link": "68",
      "is_end": false,
      "node_type": "normal",
      "children": {
        "12": {
          "id": "12",
          "speaker": "Bosun Gannet",
          "text": "Captain Grisly is dead. My only comfort is that you offed her killer - a godsdamned hag, of all things.",
          "context": "NodeContext: sad, trying to be grateful but sturggling",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "68",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "54": {
              "id": "54",
              "speaker": "Player",
              "text": "How'd she get captured in the first place?",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "68",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "24": {
                  "id": "24",
                  "speaker": "Bosun Gannet",
                  "text": "Not a clue - my head is still melted. Feels like I've been sucking on fog for days.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "68",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "36": {
                      "id": "36",
                      "speaker": "Bosun Gannet",
                      "text": "All I know is I had one job - protect my Captain. And I failed.",
                      "context": "NodeContext: disgusted with himself",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "68",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {}
                    }
                  }
                }
              }
            },
            "48": {
              "id": "48",
              "speaker": "Player",
              "text": "I'm sorry I couldn't save her.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [
                "Astarion -1",
                "Gale 1",
                "Wyll 1",
                "Lae'zel -1",
                "Shadowheart 1",
                "Karlach 1",
                "Jaheira 1",
                "Minthara -1",
