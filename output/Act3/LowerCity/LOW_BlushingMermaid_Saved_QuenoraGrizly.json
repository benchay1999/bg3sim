    "7": {
      "id": "7",
      "speaker": "Captain Grisly",
      "text": "I've raided warships. Fought every beastie the sea can throw at you, and I've never flinched - not once.",
      "context": "NodeContext: shook, vulnerable. This is the REAL captain grisly, who has been rescued from the hag by the player.",
      "checkflags": [],
      "setflags": [
