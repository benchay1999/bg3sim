                              "ruletags": [],
                              "approval": [],
                              "rolls": "",
                              "goto": "",
                              "link": "",
                              "is_end": false,
                              "node_type": "normal",
                              "children": {
                                "27": {
                                  "id": "27",
                                  "speaker": "Malta",
                                  "text": "The tall one had failed the first test: they lacked caution. The cat could have just as easily been a foe as a potential friend.",
                                  "context": "NodeContext: Intense narration",
                                  "checkflags": [],
                                  "setflags": [],
                                  "ruletags": [],
                                  "approval": [],
                                  "rolls": "",
                                  "goto": "",
                                  "link": "",
                                  "is_end": false,
                                  "node_type": "normal",
                                  "children": {
                                    "20": {
                                      "id": "20",
                                      "speaker": "Malta",
                                      "text": "Perhaps it was simply not meant to be...",
                                      "context": "NodeContext: Intense narration - slightly saddened",
                                      "checkflags": [],
                                      "setflags": [],
                                      "ruletags": [],
                                      "approval": [],
                                      "rolls": "",
                                      "goto": "",
                                      "link": "",
                                      "is_end": true,
