    "37": {
      "id": "37",
      "speaker": "Malta",
      "text": "It was another day in the city of lies. Baldur's Gate - where the people begged for justice until they starved.",
      "context": "NodeContext: Intense narration",
      "checkflags": [
        "LOW_BonecloakApothecary_HasMet_Cat03 = False",
