              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "1": {
                  "id": "1",
                  "speaker": "Mohan",
                  "text": "'Lord'. A fine title for the greatest scoundrel in town.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "11": {
                      "id": "11",
                      "speaker": "Mohan",
                      "text": "Imagine wagging your big metal willies up and down the thoroughfares and calling yourself 'Archduke'. Pathetic.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "15": {
                          "id": "15",
                          "speaker": "Mohan",
                          "text": "He needs a proper challenge. And from what I've heard, you're the person to give it to him.",
