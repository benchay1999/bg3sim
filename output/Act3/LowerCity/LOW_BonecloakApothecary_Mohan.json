                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "27": {
                      "id": "27",
                      "speaker": "Mohan",
                      "text": "I might not have exactly what you need, but I remain at your service. Have a look, at least.",
