          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "20": {
              "id": "20",
              "speaker": "Player",
              "text": "Do you mean Lord Gortash?",
              "context": "",
              "checkflags": [
                "GLO_Gortash_State_PermaDefeated = False"
