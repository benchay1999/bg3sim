      "children": {
        "14": {
          "id": "14",
          "speaker": "1f722042-94fe-4538-b686-17fb5376cbc9",
          "text": "Don't! I can't stand it!",
          "context": "NodeContext: Anguished/afraid",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
