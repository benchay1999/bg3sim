          "children": {
            "15": {
              "id": "15",
              "speaker": "1f722042-94fe-4538-b686-17fb5376cbc9",
              "text": "You... you always had such beautiful eyes...",
              "context": "NodeContext: Suddenly wistful",
              "checkflags": [],
