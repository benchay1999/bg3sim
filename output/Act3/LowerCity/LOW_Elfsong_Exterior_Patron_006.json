  "dialogue": {
    "9": {
      "id": "9",
      "speaker": "Manip Leythim",
      "text": "Hey, mate, you look pretty well geared up. Can you spare a healing potion?",
      "context": "",
      "checkflags": [
        "LOW_Elfsong_Exterior_Patron_006_HasMet = False"
      ],
      "setflags": [
