        },
        "28": {
          "id": "28",
          "speaker": "Player",
          "text": "You sound kind of messed up, to be honest.",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
