            "7": {
              "id": "7",
              "speaker": "Oloric Witmirth",
              "text": "Messed up is what I was before! I was a playwright who'd lost the will to create. Now I see just how little that matters.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
