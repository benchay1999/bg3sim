            "30": {
              "id": "30",
              "speaker": "Oloric Witmirth",
              "text": "A worried man, a playwright who'd lost the will to write. Now I worry no more! Can't write a word? What does it matter?",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
