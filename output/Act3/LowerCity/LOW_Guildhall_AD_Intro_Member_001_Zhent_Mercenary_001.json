          "is_end": false,
          "node_type": "normal",
          "children": {
            "10": {
              "id": "10",
              "speaker": "Slick Inatra",
              "text": "'Specialists'. Thieves, con-artists, and killers - nothing so special about that, friend.",
              "context": "NodeContext: Snorting, not buying it.",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
