                    "0": {
                      "id": "0",
                      "speaker": "Player",
                      "text": "There's something strange about you. Are you really a dragonborn?<br>",
                      "context": "NodeContext: love and adoration",
                      "checkflags": [
                        "LOW_HagSurvivors_State_PassivePerceptionSuccess"
