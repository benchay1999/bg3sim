                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "1": {
                      "id": "1",
                      "speaker": "Spatula Farggo",
                      "text": "No...?",
                      "context": "NodeContext: Tentatively hopeful someone might take the leeks",
                      "checkflags": [],
