      "context": "",
      "checkflags": [
        "LOW_SerialKiller_State_OnlyCora_HasMet = False",
        "LOW_SerialKiller_State_OnlyRoger_HasMet = False",
        "LOW_SerialKiller_State_HighberrysHasMetBeforeDolor = False",
