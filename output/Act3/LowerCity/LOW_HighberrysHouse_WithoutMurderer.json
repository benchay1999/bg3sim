        },
        "3": {
          "id": "3",
          "speaker": "Player",
          "text": "I don't know why he ran. Coward.",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "11": {
              "id": "11",
              "speaker": "Cora Highberry",
              "text": "Well, how odd.",
              "context": "",
              "checkflags": [],
              "setflags": [
                "LOW_SerialKiller_State_CoraSavedInStealth = False"
              ],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
