      "node_type": "normal",
      "children": {
        "2": {
          "id": "2",
          "speaker": "Kith'rak Voss",
          "text": "You ask the wrong question. It is not a matter of 'can'. It is a matter of 'must'.",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "1": {
              "id": "1",
              "speaker": "Kith'rak Voss",
              "text": "Whether by fortune or necessity, fate made its choice.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "4": {
                  "id": "4",
                  "speaker": "Kith'rak Voss",
                  "text": "Lae'zel was the opportunity we prayed for. This may be our one chance to secure githyanki freedom.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": true,
                  "node_type": "normal",
                  "children": {}
                }
              }
            }
          }
        }
      }
    },
    "9": {
      "id": "9",
      "speaker": "Kith'rak Voss",
      "text": "The time is soon upon us, Qudenos. I will not forget your loyalty, friend.",
      "context": "NodeContext: cue-DAY-nawss",
      "checkflags": [],
      "setflags": [
        "64d1f52f-7597-9615-a74b-1c3becc80609_A = False",
