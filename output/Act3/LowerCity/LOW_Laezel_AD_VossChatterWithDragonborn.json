          "node_type": "normal",
          "children": {
            "6": {
              "id": "6",
              "speaker": "Qudenos",
              "text": "You will be the first to battle. And I will be the one to carry you.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": true,
