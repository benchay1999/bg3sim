      "node_type": "normal",
      "children": {
        "8": {
          "id": "8",
          "speaker": "Qudenos",
          "text": "The lich queen has shackled the red dragons as surely as she has shackled the githyanki.",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
