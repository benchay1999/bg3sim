          "children": {
            "10": {
              "id": "10",
              "speaker": "Kith'rak Voss",
              "text": "For a thousand years, we have been planting the seeds of revolution.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
