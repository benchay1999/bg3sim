              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "2": {
                  "id": "2",
                  "speaker": "Pompton Norvath",
                  "text": "Opportunists, the lot of them. And not a thought for their neighbours.",
                  "context": "NodeContext: Bitterly",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
