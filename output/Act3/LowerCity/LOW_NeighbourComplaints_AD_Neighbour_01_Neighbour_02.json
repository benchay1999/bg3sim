      "node_type": "normal",
      "children": {
        "3": {
          "id": "3",
          "speaker": "Pompton Norvath",
          "text": "Louder than foxes, and all through the night!",
          "context": "NodeContext: Gossipy (and enjoying it)",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
