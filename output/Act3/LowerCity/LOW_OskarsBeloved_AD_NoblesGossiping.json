          "node_type": "normal",
          "children": {
            "4": {
              "id": "4",
              "speaker": "Saer Dokksill",
              "text": "Mine travelled all the way from Neverwinter, to be turned away at the gate. It's outrageous.",
              "context": "NodeContext: Deadly serious, this is a BIG issue",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
