      "children": {
        "7": {
          "id": "7",
          "speaker": "Saer Dokksill",
          "text": "I think not. How is one to throw a half-decent party without a florist?",
          "context": "NodeContext: Deadly serious, this is a BIG issue",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
