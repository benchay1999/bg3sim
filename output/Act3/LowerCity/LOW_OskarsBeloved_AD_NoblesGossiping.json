              "is_end": false,
              "node_type": "normal",
              "children": {
                "12": {
                  "id": "12",
                  "speaker": "Impurto Nexim",
                  "text": "You must take this to Lord Gortash. He's a man who will see sense.",
                  "context": "NodeContext: Outraged at what he's just heard",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
