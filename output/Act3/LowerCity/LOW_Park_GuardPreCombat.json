                    "1": {
                      "id": "1",
                      "speaker": "Manip Edenosa",
                      "text": "Not so loud. You and I might have an understanding, but that doesn't go for everyone. Plenty of my comrades still serve the old order.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
