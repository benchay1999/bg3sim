      "is_end": false,
      "node_type": "normal",
      "children": {
        "10": {
          "id": "10",
          "speaker": "Cora Highberry",
          "text": "Our coinpurse, dear.",
          "context": "",
          "checkflags": [],
          "setflags": [],
