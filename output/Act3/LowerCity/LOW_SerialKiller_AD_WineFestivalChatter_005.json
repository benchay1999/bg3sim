              "is_end": false,
              "node_type": "normal",
              "children": {
                "5": {
                  "id": "5",
                  "speaker": "Meztli",
                  "text": "Well, Lliira herself would smite me where I stood.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [
