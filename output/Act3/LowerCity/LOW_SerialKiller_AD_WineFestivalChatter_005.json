    "9": {
      "id": "9",
      "speaker": "Roger Highberry",
      "text": "So, how much will this tasting session lighten my coinpurse?",
      "context": "",
      "checkflags": [
        "6a24dd19-c1ee-9515-56ee-8c92c5d686c8_B"
      ],
