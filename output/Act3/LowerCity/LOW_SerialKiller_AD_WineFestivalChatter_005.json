    "7": {
      "id": "7",
      "speaker": "Meztli",
      "text": "My, what a handsome couple you make! May I interest you both in a tasting?",
      "context": "",
      "checkflags": [],
      "setflags": [],
      "ruletags": [],
      "approval": [],
      "rolls": "",
      "goto": "",
      "link": "",
      "is_end": false,
      "node_type": "normal",
      "children": {
        "14": {
          "id": "14",
          "speaker": "Cora Highberry",
          "text": "Gosh - I must say, flattery will get you everywhere!",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "8": {
              "id": "8",
              "speaker": "Roger Highberry",
              "text": "It's certainly worked for me over the last few decades, my love!",
              "context": "",
              "checkflags": [],
              "setflags": [
