    "12": {
      "id": "12",
      "speaker": "Ratin Sesescor",
      "text": "Hello, hello. Are you the new cheese-dropper? I do hope so - I'm almost down to the last crumbs.",
      "context": "",
      "checkflags": [
        "LOW_SerialKiller_RatWitness_HasMet = False",
        "PETPAL"
      ],
