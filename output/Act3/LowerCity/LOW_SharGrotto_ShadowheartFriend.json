        "11": {
          "id": "11",
          "speaker": "Player",
          "text": "I don't suppose there are any pleasant memories you could share?",
          "context": "NodeContext: Buh-dug",
          "checkflags": [
            "LOW_SharGrotto_ShadowheartFriend_Event_Reminisced",
            "LOW_SharGrotto_ShadowheartFriend_Event_ReminscedContinuation = False",
            "REALLY_SHADOWHEART"
          ],
