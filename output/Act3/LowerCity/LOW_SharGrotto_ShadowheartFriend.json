          "children": {
            "12": {
              "id": "12",
              "speaker": "Nocturne",
              "text": "Please - don't call me 'friend'. You know what it's like here - backstabbing, vying for favour.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
