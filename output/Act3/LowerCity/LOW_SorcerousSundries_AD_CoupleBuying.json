              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "13": {
                  "id": "13",
                  "speaker": "Lester Farrower",
                  "text": "Not everyone can just wave their hands and be healed. Why aren't they rationing anything?",
                  "context": "NodeContext: Annoyed",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "8": {
                      "id": "8",
                      "speaker": "Shuna Huanje",
                      "text": "Gold. It's all about profits, my love.",
                      "context": "NodeContext: Matter of factly",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
