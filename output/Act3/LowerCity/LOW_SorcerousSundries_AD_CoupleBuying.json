          "children": {
            "17": {
              "id": "17",
              "speaker": "Lester Farrower",
              "text": "Bloodstaunch, jewelweed, mothersleaf - all gone. Typical.",
              "context": "NodeContext: Reeling off a list in exasperation; it's all gone",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
