      "is_end": false,
      "node_type": "normal",
      "children": {
        "15": {
          "id": "15",
          "speaker": "Shuna Huanje",
          "text": "Some people are so selfish.",
          "context": "NodeContext: Frustrated/disapproving",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
