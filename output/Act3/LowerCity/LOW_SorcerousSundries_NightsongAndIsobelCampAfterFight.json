      "is_end": false,
      "node_type": "normal",
      "children": {
        "14": {
          "id": "14",
          "speaker": "Nightsong",
          "text": "I was just regaling sweet Isobel with tales of our prowess.",
          "context": "NodeContext: Warm, but drained. She's not feeling like herself.",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
