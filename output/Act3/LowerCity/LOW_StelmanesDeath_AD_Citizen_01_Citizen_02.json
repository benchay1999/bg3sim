    "2": {
      "id": "2",
      "speaker": "Hilario Varota",
      "text": "First Stelmane, and now Gortash! If the higher-ups aren't safe, what hope do we have?",
      "context": "",
      "checkflags": [
        "GLO_Gortash_State_PermaDefeated"
      ],
      "setflags": [
        "daa914ef-2c71-859d-8f13-e44b9de19b1c_A = False",
