          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "14": {
              "id": "14",
              "speaker": "Hilario Varota",
              "text": "Probably responsible for his death, if you ask me.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "8": {
                  "id": "8",
                  "speaker": "Megthanet",
                  "text": "I always said he was doing too good a job. Better than Stelmane, in any case.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
