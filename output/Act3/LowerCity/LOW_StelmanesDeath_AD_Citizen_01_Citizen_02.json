      "link": "",
      "is_end": false,
      "node_type": "normal",
      "children": {
        "12": {
          "id": "12",
          "speaker": "Megthanet",
          "text": "Whoever it is, good luck to them. Those Absolutists won't stop just because there's a power vacuum.",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
