        "59": {
          "id": "59",
          "speaker": "Player",
          "text": "You should have killed Wulbren.",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [
            "Astarion 1",
            "Gale -1",
            "Shadowheart 1",
