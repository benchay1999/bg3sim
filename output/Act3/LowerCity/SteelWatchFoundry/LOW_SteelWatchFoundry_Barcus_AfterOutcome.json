        "47": {
          "id": "47",
          "speaker": "Player",
          "text": "You stood up to Wulbren - I'm impressed.",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [
            "Astarion 1",
            "Gale 1",
