              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "14": {
                  "id": "14",
                  "speaker": "Carmen Pennygood",
                  "text": "But if it's genuine quality, and genuine taste,  you're after - Carm's Garms has you covered.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
