        "41": {
          "id": "41",
          "speaker": "Player",
          "text": "You're a lot less snooty than that other clothes seller, Figaro.",
          "context": "",
          "checkflags": [
            "LOW_Serialkiller_State_FigaroWhoIs",
            "WYR_FigaroSister_Carmen_Knows_FigaroIsDead = False",
