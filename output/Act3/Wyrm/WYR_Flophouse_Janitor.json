  "dialogue": {
    "8": {
      "id": "8",
      "speaker": "Dashkent Moliver",
      "text": "Sir, good sir. Give me three days, and -",
      "context": "CinematicNodeContext: Dashkent not making eye contact, looking at floor and fidgeting, assuming you are someone else - the landlord.",
      "checkflags": [],
      "setflags": [
        "WYR_Flophouse_HasMet_Janitor"
      ],
      "ruletags": [],
      "approval": [],
      "rolls": "",
      "goto": "",
      "link": "19",
      "is_end": false,
      "node_type": "normal",
      "children": {
        "29": {
          "id": "29",
          "speaker": "Dashkent Moliver",
          "text": "Oh, erm - apologies. Thought you were someone else. Greetings, so forth, so on.",
          "context": "CinematicNodeContext: Dashkent makes eye contact with player and realises his mistake.",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "19",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "28": {
              "id": "28",
              "speaker": "Player",
              "text": "Ffion had a son. Did you two ever meet?",
              "context": "",
              "checkflags": [
                "WYR_Flophouse_Knows_MotherKilledBySon"
              ],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "19",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "25": {
                  "id": "25",
                  "speaker": "Dashkent Moliver",
                  "text": "Dolor? Weird fella. He'd come and go, quiet as a bug's breath.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "13": {
                      "id": "13",
                      "speaker": "Dashkent Moliver",
                      "text": "Never here when his mum needed him. Always here when she didn't.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {}
                    }
                  }
                }
              }
            },
            "7": {
              "id": "7",
              "speaker": "Player",
              "text": "I found a dwarf's corpse hidden away upstairs. What do you know about it?",
              "context": "",
              "checkflags": [
                "WYR_Flophouse_State_FoundMother",
                "3f106086-a1e6-8201-802a-5faaf4469056_TalkedAboutFionn = False"
              ],
              "setflags": [
                "WYR_Flophouse_Janitor_Event_KnowsFfionDead"
              ],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "19",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "14": {
                  "id": "14",
                  "speaker": "Dashkent Moliver",
                  "text": "Oh. Oh dear. A dwarf, you say? Ffion, one of Mamzell's girls from Sharess' Caress!",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "19",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "2": {
                      "id": "2",
                      "speaker": "Dashkent Moliver",
                      "text": "Been gone for a tenday. Smiled as wide as the Wide, but twice as sunny.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "19",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "31": {
                          "id": "31",
                          "speaker": "Dashkent Moliver",
                          "text": "You don't suppose the murderer - oh goodness no, it's too horrible to imagine!",
                          "context": "",
                          "checkflags": [],
                          "setflags": [],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
                          "goto": "",
                          "link": "19",
                          "is_end": false,
                          "node_type": "normal",
                          "children": {}
                        }
                      }
                    }
                  }
                }
              }
            },
            "5": {
              "id": "5",
              "speaker": "Player",
              "text": "Ffion's been murdered. I found her corpse.",
              "context": "",
              "checkflags": [
                "WYR_Flophouse_State_FoundMother",
                "3f106086-a1e6-8201-802a-5faaf4469056_TalkedAboutFionn"
              ],
              "setflags": [
                "WYR_Flophouse_Janitor_Event_KnowsFfionDead"
              ],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "17": {
                  "id": "17",
                  "speaker": "Dashkent Moliver",
                  "text": "Oh - oh dear. Poor thing. Murdered to death!",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "19": {
                      "id": "19",
                      "speaker": "Dashkent Moliver",
                      "text": "Horrible Hells, I better lay low. Lower than low. Underlow.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [
                        "WYR_Flophouse_Janitor_Event_KnowsFfionDead"
                      ],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": true,
                      "node_type": "normal",
                      "children": {}
                    }
                  }
                }
              }
            },
            "26": {
              "id": "26",
              "speaker": "Player",
              "text": "What do you know about Ffion Goldgrind?",
              "context": "",
              "checkflags": [
                "WYR_SharessCaress_FindingFionn_State_AcceptedQuest",
                "3f106086-a1e6-8201-802a-5faaf4469056_TalkedAboutFionn = False"
