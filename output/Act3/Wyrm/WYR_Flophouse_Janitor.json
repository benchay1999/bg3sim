              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "1": {
                  "id": "1",
                  "speaker": "Dashkent Moliver",
                  "text": "Ffion - one of Mamzell's girls! Laughs like a star's twinkle.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "10": {
                      "id": "10",
                      "speaker": "Dashkent Moliver",
                      "text": "Not seen her for a tenday. Ten days and four bells, to be precise. Not that I'm counting.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
