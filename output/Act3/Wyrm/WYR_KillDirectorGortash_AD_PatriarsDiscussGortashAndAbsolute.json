          "children": {
            "2": {
              "id": "2",
              "speaker": "Lady Beatrice Provoss",
              "text": "The refreshments are fine, but I won't feel safe until I'm back in the Upper City. Behind solid walls.",
              "context": "NodeContext: slightly agitated",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
