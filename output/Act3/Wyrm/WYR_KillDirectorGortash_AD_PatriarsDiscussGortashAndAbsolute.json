  "dialogue": {
    "3": {
      "id": "3",
      "speaker": "Lady Beatrice Provoss",
      "text": "After enduring Ravengard's slovenly manners for so long, I was rather hoping our next duke might be of high-born stock.",
      "context": "NodeContext: pompous rich noble",
      "checkflags": [],
      "setflags": [
        "157d18aa-a190-6742-8099-18c8928bac5f_A"
