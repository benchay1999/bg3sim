          "is_end": false,
          "node_type": "normal",
          "children": {
            "30": {
              "id": "30",
              "speaker": "Zorru",
              "text": "It's all right, I'm not mad about it anymore. He tried his best to lead us in a terrible situation. I see that now.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": true,
