          "children": {
            "33": {
              "id": "33",
              "speaker": "Zorru",
              "text": "That loser? Don't bother. I don't even want to think about him after what he did.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
