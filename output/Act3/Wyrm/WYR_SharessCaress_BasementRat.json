          "children": {
            "12": {
              "id": "12",
              "speaker": "Rat",
              "text": "Then... you're one of us? You believe in the equal distribution of food regardless of species?",
              "context": "NodeContext: Tentatively; believes the player might on their side",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "25": {
                  "id": "25",
                  "speaker": "Player",
                  "text": "Sure. Now let's grab what we can and get out of here.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "23": {
                      "id": "23",
                      "speaker": "Rat",
                      "text": "Yes! Victuals for all! May you brux until your eyes boggle!",
                      "context": "NodeContext: Happy that the player is on their side; zealous celebration",
                      "checkflags": [],
                      "setflags": [
                        "WYR_SharessCaress_State_AgreedWithRat"
                      ],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": true,
                      "node_type": "normal",
                      "children": {}
                    }
                  }
                },
                "8": {
                  "id": "8",
                  "speaker": "Player",
                  "text": "Oh yes. And you should definitely rise and overthrow those oppressors while you're at it.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "22": {
                      "id": "22",
                      "speaker": "Rat",
                      "text": "We shall! We will fly in the face of the oppressors and their foolish laws! Leave no crust unnibbled!",
                      "context": "NodeContext: Zealous celebration",
                      "checkflags": [],
                      "setflags": [
                        "WYR_SharessCaress_State_AgreedWithRat"
                      ],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": true,
                      "node_type": "normal",
                      "children": {}
                    }
                  }
                },
                "20": {
                  "id": "20",
                  "speaker": "Player",
                  "text": "Leave.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": true,
                  "node_type": "normal",
                  "children": {}
                }
              }
            }
          }
