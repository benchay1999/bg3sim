                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "11": {
                      "id": "11",
                      "speaker": "Lady Durinbold's Bodyguard",
                      "text": "Ravengard is a charlatan. Like every last patriar of this wretched city.",
