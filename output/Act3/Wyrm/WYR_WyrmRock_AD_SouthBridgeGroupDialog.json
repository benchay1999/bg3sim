    "5": {
      "id": "5",
      "speaker": "Fist Shah",
      "text": "Lord Gortash is dead! Why are we still standing guard here like nothing's happened?",
      "context": "",
      "checkflags": [
        "GLO_Gortash_State_WorldKnowsPermaDefeated"
      ],
      "setflags": [],
      "ruletags": [],
      "approval": [],
      "rolls": "",
      "goto": "",
      "link": "",
      "is_end": false,
      "node_type": "normal",
      "children": {
        "11": {
          "id": "11",
          "speaker": "Blaze Elin",
          "text": "Your orders haven't changed, and until they do, nothing has happened.",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "13": {
              "id": "13",
              "speaker": "Fist Shah",
              "text": "And you think that makes sense?",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "7": {
                  "id": "7",
                  "speaker": "Blaze Elin",
                  "text": "I do, but then I'm paid to think. You aren't. Shut up, hold your weapon straight, and keep your eyes open.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": true,
                  "node_type": "normal",
                  "children": {}
                }
              }
            }
          }
        }
      }
    },
    "12": {
