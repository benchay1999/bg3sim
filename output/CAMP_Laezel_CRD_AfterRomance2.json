                          "is_end": false,
                          "node_type": "normal",
                          "children": {
                            "19": {
                              "id": "19",
                              "speaker": "Lae'zel",
                              "text": "As have I.",
                              "context": "",
                              "checkflags": [],
                              "setflags": [],
                              "ruletags": [],
                              "approval": [],
                              "rolls": "",
                              "goto": "",
                              "link": "",
                              "is_end": false,
                              "node_type": "normal",
                              "children": {
                                "3": {
                                  "id": "3",
                                  "speaker": "Lae'zel",
                                  "text": "Truthfully, it took more courage to ask for a soft touch than it would to meet the gaze of the Ninefold Dragon.",
                                  "context": "",
                                  "checkflags": [],
                                  "setflags": [],
                                  "ruletags": [],
                                  "approval": [],
                                  "rolls": "",
                                  "goto": "",
                                  "link": "",
                                  "is_end": false,
                                  "node_type": "normal",
                                  "children": {
                                    "16": {
                                      "id": "16",
                                      "speaker": "Lae'zel",
                                      "text": "Thus far I've taunted you, devoured you, battled you.",
                                      "context": "",
                                      "checkflags": [],
                                      "setflags": [],
                                      "ruletags": [],
                                      "approval": [],
                                      "rolls": "",
                                      "goto": "",
                                      "link": "",
                                      "is_end": false,
                                      "node_type": "normal",
                                      "children": {
                                        "21": {
                                          "id": "21",
                                          "speaker": "Lae'zel",
                                          "text": "Now, I want more than anything to soothe you.",
                                          "context": "",
                                          "checkflags": [],
                                          "setflags": [],
                                          "ruletags": [],
                                          "approval": [],
                                          "rolls": "",
                                          "goto": "",
                                          "link": "",
