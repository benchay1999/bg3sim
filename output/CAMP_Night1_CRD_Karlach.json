        "15": {
          "id": "15",
          "speaker": "Player",
          "text": "Where'd you sleep in the Hells?",
          "context": "NodeContext: Good-humoured.",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "21",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "13": {
              "id": "13",
              "speaker": "Karlach",
              "text": "Rough. You've seen Avernus - nothing but rocks and evil pricks as far as the eye can see.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "21",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "28": {
                  "id": "28",
                  "speaker": "Karlach",
                  "text": "But this - a fire, a bit of nature, and a spot to call my own - this is nice.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "21",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {}
                }
              }
            }
          }
        },
        "19": {
          "id": "19",
          "speaker": "Player",
          "text": "It's been a while for me, too. This is all so unusual.",
          "context": "NodeContext: Good-humoured.",
          "checkflags": [],
