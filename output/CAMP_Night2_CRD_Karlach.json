                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "2",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "18": {
                      "id": "18",
                      "speaker": "Player",
                      "text": "After where you've been, I'm not surprised this place seems like heaven.",
