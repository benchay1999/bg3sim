        "14": {
          "id": "14",
          "speaker": "Player",
          "text": "All I hear is you.",
          "context": "CinematicNodeContext: Her eyes snap open\rNodeContext: Laughingly",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "2",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "5": {
              "id": "5",
              "speaker": "Karlach",
              "text": "Well, I'm certainly part of it! The breeze, the fire, me big stupid head. I love it here. So much.",
              "context": "CinematicNodeContext: Her eyes snap open\rNodeContext: Laughingly",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "2",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "13": {
                  "id": "13",
                  "speaker": "Karlach",
                  "text": "This camp. Faer\u00fbn. Toril. The Material Plane. Paradise.",
