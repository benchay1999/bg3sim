                },
                "24": {
                  "id": "24",
                  "speaker": "Player",
                  "text": "Confident. No tiny tadpole will be getting the best of me.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
