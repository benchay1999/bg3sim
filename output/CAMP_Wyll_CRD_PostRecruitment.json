                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "3": {
                      "id": "3",
                      "speaker": "Wyll",
                      "text": "Courage in danger's shadow. I love to see it.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "5",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "17": {
                          "id": "17",
                          "speaker": "Wyll",
                          "text": "But shouldn't the shadow have crept closer, gone darker by now?",
                          "context": "",
                          "checkflags": [],
                          "setflags": [],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
                          "goto": "",
                          "link": "5",
                          "is_end": false,
