                                    "22": {
                                      "id": "22",
                                      "speaker": "Player",
                                      "text": "We could probably dig up some poison mushrooms, if you're that keen to be sick.",
                                      "context": "NodeContext: Good natured-ly",
                                      "checkflags": [],
                                      "setflags": [],
                                      "ruletags": [],
                                      "approval": [
                                        "Wyll -1"
                                      ],
                                      "rolls": "",
                                      "goto": "",
                                      "link": "",
                                      "is_end": false,
                                      "node_type": "normal",
                                      "children": {
                                        "2": {
                                          "id": "2",
                                          "speaker": "Wyll",
                                          "text": "Heh. I speak out of curiosity, not wishful thinking. At least I'll know to skip supper if you're doing the cooking.",
                                          "context": "NodeContext: Good natured-ly",
                                          "checkflags": [],
                                          "setflags": [],
