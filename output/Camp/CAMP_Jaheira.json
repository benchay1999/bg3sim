                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {}
                },
                "52": {
                  "id": "52",
                  "speaker": "Jaheira",
                  "text": "You know your task. Ketheric Thorm clawed his way free of death. Find out how, so we can send him back.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {}
                }
              }
            },
            "35": {
              "id": "35",
              "speaker": "Player",
              "text": "Leave.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
