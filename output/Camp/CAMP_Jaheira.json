                  "node_type": "normal",
                  "children": {
                    "51": {
                      "id": "51",
                      "speaker": "Jaheira",
                      "text": "But first - I owe them justice. Moonrise must fall.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
