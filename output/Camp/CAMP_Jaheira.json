              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "22": {
                  "id": "22",
                  "speaker": "Jaheira",
                  "text": "The time for words will come, when we lay my Harpers to their final rest. I owe them that.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
