            "24": {
              "id": "24",
              "speaker": "Player",
              "text": "So. What now?",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "11": {
                  "id": "11",
                  "speaker": "Jaheira",
                  "text": "You have already made that choice. You returned Ketheric's relic to him. Perhaps you won his favour - but now you must face the full force of his power, alone.",
                  "context": "",
                  "checkflags": [
                    "HAV_Jaheira_Knows_SentNightsongBackToKetheric"
