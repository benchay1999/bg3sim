                            "4": {
                              "id": "4",
                              "speaker": "Gale",
                              "text": "'Go to Hell.' An everyday expression. So trivial it's almost meaningless.",
                              "context": "",
                              "checkflags": [],
                              "setflags": [],
                              "ruletags": [],
                              "approval": [],
                              "rolls": "",
                              "goto": "",
