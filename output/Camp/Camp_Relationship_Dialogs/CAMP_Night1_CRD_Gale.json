                              "is_end": false,
                              "node_type": "normal",
                              "children": {
                                "17": {
                                  "id": "17",
                                  "speaker": "Gale",
                                  "text": "But we've seen Hell. It's real. And it isn't trivial.",
                                  "context": "",
                                  "checkflags": [],
                                  "setflags": [],
                                  "ruletags": [],
                                  "approval": [],
                                  "rolls": "",
                                  "goto": "",
                                  "link": "10",
                                  "is_end": false,
                                  "node_type": "normal",
                                  "children": {
                                    "28": {
                                      "id": "28",
                                      "speaker": "Player",
                                      "text": "What's on your mind, Gale?",
