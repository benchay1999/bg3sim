                                    "24": {
                                      "id": "24",
                                      "speaker": "Player",
                                      "text": "Watch the flames in silence.",
                                      "context": "",
                                      "checkflags": [],
                                      "setflags": [],
                                      "ruletags": [],
                                      "approval": [],
                                      "rolls": "",
                                      "goto": "",
                                      "link": "8",
                                      "is_end": false,
                                      "node_type": "normal",
                                      "children": {
                                        "10": {
                                          "id": "10",
                                          "speaker": "Gale",
                                          "text": "Devils, dragons, mind flayers - they used to be abstracts. Pictures on a piece of paper.",
                                          "context": "",
                                          "checkflags": [],
                                          "setflags": [],
                                          "ruletags": [],
                                          "approval": [],
                                          "rolls": "",
                                          "goto": "",
                                          "link": "8",
                                          "is_end": false,
                                          "node_type": "normal",
                                          "children": {
                                            "2": {
                                              "id": "2",
                                              "speaker": "Gale",
                                              "text": "What a difference a day makes.",
                                              "context": "",
                                              "checkflags": [],
                                              "setflags": [],
                                              "ruletags": [],
                                              "approval": [],
                                              "rolls": "",
                                              "goto": "",
                                              "link": "8",
                                              "is_end": false,
                                              "node_type": "normal",
                                              "children": {
                                                "29": {
                                                  "id": "29",
                                                  "speaker": "Gale",
                                                  "text": "Now we have tadpoles slithering through our heads like carnivorous foeti.",
                                                  "context": "",
                                                  "checkflags": [],
                                                  "setflags": [],
                                                  "ruletags": [],
                                                  "approval": [],
                                                  "rolls": "",
                                                  "goto": "",
                                                  "link": "8",
                                                  "is_end": false,
                                                  "node_type": "normal",
                                                  "children": {
                                                    "19": {
