                                      "checkflags": [],
                                      "setflags": [],
                                      "ruletags": [],
                                      "approval": [],
                                      "rolls": "",
                                      "goto": "",
                                      "link": "10",
                                      "is_end": false,
                                      "node_type": "normal",
                                      "children": {}
