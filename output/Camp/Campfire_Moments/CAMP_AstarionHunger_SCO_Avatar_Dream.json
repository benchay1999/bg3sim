                          "children": {}
                        },
                        "1": {
                          "id": "1",
                          "speaker": "Astarion",
                          "text": "Where are you?  Show yourself!",
                          "context": "",
                          "checkflags": [
                            "927f5d77-9be4-3c73-672d-f34b04c16365_ReadyToFight"
                          ],
