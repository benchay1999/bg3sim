                          "node_type": "normal",
                          "children": {
                            "17": {
                              "id": "17",
                              "speaker": "Cazador Szarr",
                              "text": "Third, thou shalt not leave my side unless directed.",
                              "context": "",
                              "checkflags": [],
                              "setflags": [],
                              "ruletags": [],
                              "approval": [],
                              "rolls": "",
                              "goto": "",
                              "link": "4",
                              "is_end": false,
                              "node_type": "normal",
                              "children": {
                                "0": {
                                  "id": "0",
                                  "speaker": "Narrator",
                                  "text": "*You can feel the old fear turning in your gut, but you force yourself to keep smiling. As you so often did before.*",
                                  "context": "",
                                  "checkflags": [
                                    "927f5d77-9be4-3c73-672d-f34b04c16365_Froze"
                                  ],
                                  "setflags": [],
