                                  "is_end": false,
                                  "node_type": "normal",
                                  "children": {
                                    "19": {
                                      "id": "19",
                                      "speaker": "Cazador Szarr",
                                      "text": "Fourth, thou shalt know that thou art mine.",
                                      "context": "",
                                      "checkflags": [],
                                      "setflags": [],
                                      "ruletags": [],
                                      "approval": [],
                                      "rolls": "",
                                      "goto": "",
                                      "link": "4",
                                      "is_end": false,
                                      "node_type": "normal",
                                      "children": {
                                        "9": {
                                          "id": "9",
                                          "speaker": "Astarion",
                                          "text": "Not anymore. I'm free now.",
                                          "context": "",
                                          "checkflags": [],
                                          "setflags": [],
                                          "ruletags": [],
                                          "approval": [],
                                          "rolls": "",
                                          "goto": "",
                                          "link": "4",
                                          "is_end": false,
                                          "node_type": "normal",
                                          "children": {
                                            "11": {
                                              "id": "11",
                                              "speaker": "Cazador Szarr",
                                              "text": "Free? Lie to yourself, boy, but not to me.",
                                              "context": "",
                                              "checkflags": [],
                                              "setflags": [],
                                              "ruletags": [],
                                              "approval": [],
                                              "rolls": "",
                                              "goto": "",
                                              "link": "4",
                                              "is_end": false,
                                              "node_type": "normal",
                                              "children": {}
                                            }
                                          }
                                        },
                                        "8": {
                                          "id": "8",
                                          "speaker": "Astarion",
                                          "text": "Please, you have to forgive me.",
                                          "context": "",
                                          "checkflags": [],
                                          "setflags": [],
                                          "ruletags": [],
                                          "approval": [],
                                          "rolls": "",
                                          "goto": "",
                                          "link": "4",
                                          "is_end": false,
                                          "node_type": "normal",
                                          "children": {
                                            "2": {
                                              "id": "2",
                                              "speaker": "Cazador Szarr",
                                              "text": "Pathetic. As always. But at least you know your place.",
                                              "context": "",
                                              "checkflags": [],
                                              "setflags": [],
                                              "ruletags": [],
                                              "approval": [],
                                              "rolls": "",
                                              "goto": "",
                                              "link": "",
                                              "is_end": false,
                                              "node_type": "normal",
                                              "children": {
                                                "4": {
                                                  "id": "4",
                                                  "speaker": "Cazador Szarr",
                                                  "text": "You are mine. Forever.",
                                                  "context": "",
