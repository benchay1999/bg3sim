                                          "rolls": "",
                                          "goto": "",
                                          "link": "4",
                                          "is_end": false,
                                          "node_type": "normal",
                                          "children": {
                                            "6": {
                                              "id": "6",
                                              "speaker": "Narrator",
                                              "text": "*You swing, but connect with nothing.*",
                                              "context": "",
                                              "checkflags": [],
                                              "setflags": [],
                                              "ruletags": [],
                                              "approval": [],
                                              "rolls": "",
                                              "goto": "",
