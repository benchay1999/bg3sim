              "is_end": false,
              "node_type": "normal",
              "children": {
                "7": {
                  "id": "7",
                  "speaker": "Astarion",
                  "text": "Master? I was just coming to see you!",
                  "context": "",
                  "checkflags": [],
                  "setflags": [
