      ],
      "setflags": [
        "f2bfa062-4d14-9d5c-f104-79b5a5f91227_A = False"
      ],
      "ruletags": [],
      "approval": [],
      "rolls": "",
      "goto": "",
      "link": "",
      "is_end": false,
      "node_type": "normal",
      "children": {
        "7": {
          "id": "7",
          "speaker": "Volo",
          "text": "Well... discretion guides me. But so does my duty to enthral the reader - to swell their hearts and dampen their cheeks!",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "5": {
              "id": "5",
              "speaker": "Weary Traveller",
              "text": "Moisten whatever you like, so long as you do not share dangerous secrets.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": true,
              "node_type": "normal",
              "children": {}
            }
          }
        }
      }
    },
    "11": {
      "id": "11",
      "speaker": "Volo",
      "text": "Oh-ho, you scallawag! Why, your editor's quill made me into the sterling scribe I am today.",
