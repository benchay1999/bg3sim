      "setflags": [
        "f2bfa062-4d14-9d5c-f104-79b5a5f91227_B"
      ],
      "ruletags": [],
      "approval": [],
      "rolls": "",
      "goto": "",
      "link": "",
      "is_end": false,
      "node_type": "normal",
      "children": {
        "10": {
          "id": "10",
          "speaker": "Weary Traveller",
          "text": "The harm could be great. Few things are more dangerous than a fool who thinks himself enlightened.",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "0": {
              "id": "0",
              "speaker": "Volo",
              "text": "I see. Well luckily I cannot claim to know any such unfortunate.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": true,
              "node_type": "normal",
              "children": {}
            }
          }
        }
      }
    },
    "8": {
      "id": "8",
      "speaker": "Weary Traveller",
      "text": "Wonderful. And I trust you remain more... discreet about sharing sensitive knowledge?",
      "context": "NodeContext: A bit sarcasitc on the 'wonderful'",
      "checkflags": [
        "f2bfa062-4d14-9d5c-f104-79b5a5f91227_B",
