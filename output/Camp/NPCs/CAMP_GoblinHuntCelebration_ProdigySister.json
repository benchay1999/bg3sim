              "is_end": false,
              "node_type": "normal",
              "children": {
                "4": {
                  "id": "4",
                  "speaker": "Lia",
                  "text": "Look us up when you hit Baldur's Gate. Be nice to see a friendly face.",
                  "context": "NodeContext: Sincere and heartfelt",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
