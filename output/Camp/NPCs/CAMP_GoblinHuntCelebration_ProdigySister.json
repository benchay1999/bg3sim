            "12": {
              "id": "12",
              "speaker": "Lia",
              "text": "Yeah. Don't tell Rolan, but his magic show might be the highlight. He's been doing them since we were little.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
