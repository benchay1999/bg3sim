          "node_type": "normal",
          "children": {
            "0": {
              "id": "0",
              "speaker": "Lia",
              "text": "Heh. Then I'd best track down a bottle - before the rest polish it off.",
              "context": "NodeContext: Sincere and heartfelt",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
