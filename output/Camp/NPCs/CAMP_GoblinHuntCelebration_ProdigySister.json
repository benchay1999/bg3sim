            "9": {
              "id": "9",
              "speaker": "Lia",
              "text": "Me too. You're one of the good ones, you know that?",
              "context": "NodeContext: Sincere and heartfelt",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
