                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "21": {
                          "id": "21",
                          "speaker": "Astarion",
                          "text": "But now that we know it's safe, I am all for experimenting.",
                          "context": "",
                          "checkflags": [],
                          "setflags": [],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
                          "goto": "",
                          "link": "",
                          "is_end": false,
                          "node_type": "normal",
                          "children": {}
                        }
                      }
                    }
                  }
                },
                "27": {
                  "id": "27",
                  "speaker": "Player",
