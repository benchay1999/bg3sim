  "dialogue": {
    "5": {
      "id": "5",
      "speaker": "Astarion",
      "text": "You know, if that parasite isn't to your taste, I'd be happy to consume it in your stead.",
      "context": "NodeContext: A hint of threat",
      "checkflags": [
        "ORI_Astarion_IPRD_TadpolePowers",
        "GLO_Tadpole_Event_FirstConsumed = False"
      ],
