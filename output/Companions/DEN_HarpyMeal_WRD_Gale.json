                                  "children": {
                                    "7": {
                                      "id": "7",
                                      "speaker": "Gale",
                                      "text": "Of course, in walked the housekeeper. Screaming, yelling, panic, and before you knew it: fire everywhere.",
                                      "context": "",
                                      "checkflags": [],
                                      "setflags": [],
                                      "ruletags": [],
                                      "approval": [],
                                      "rolls": "",
                                      "goto": "",
                                      "link": "",
                                      "is_end": false,
                                      "node_type": "normal",
                                      "children": {
                                        "14": {
                                          "id": "14",
                                          "speaker": "Gale",
                                          "text": "Anyway, I'm glad we got that boy out of his predicament.",
                                          "context": "",
                                          "checkflags": [],
                                          "setflags": [],
                                          "ruletags": [],
                                          "approval": [],
                                          "rolls": "",
                                          "goto": "",
                                          "link": "",
                                          "is_end": false,
                                          "node_type": "normal",
                                          "children": {
                                            "29": {
                                              "id": "29",
                                              "speaker": "Gale",
                                              "text": "Poor lad would have been harpy feed if it wasn't for you.",
                                              "context": "",
