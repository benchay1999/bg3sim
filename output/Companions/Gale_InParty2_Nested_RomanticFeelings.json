        "44": {
          "id": "44",
          "speaker": "Player",
          "text": "I wish I could kiss you.",
          "context": "",
          "checkflags": [
            "GLO_ForgingOfTheHeart_State_KarlachSecondUpgrade = False",
            "REALLY_KARLACH"
          ],
          "setflags": [],
