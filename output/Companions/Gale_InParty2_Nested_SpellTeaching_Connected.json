          "node_type": "normal",
          "children": {
            "8": {
              "id": "8",
              "speaker": "Gale",
              "text": "Why would you even bring this up? Come, let's go. I'm not in the mood for chatter.",
              "context": "NodeContext: irked and dismissive",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
