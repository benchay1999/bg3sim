      "is_end": false,
      "node_type": "normal",
      "children": {
        "2": {
          "id": "2",
          "speaker": "Gale",
          "text": "That I still looked as handsome as ever, that's what.",
          "context": "NodeContext: irked",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
