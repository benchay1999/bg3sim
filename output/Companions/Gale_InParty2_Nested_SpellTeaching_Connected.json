      "is_end": false,
      "node_type": "normal",
      "children": {
        "14": {
          "id": "14",
          "speaker": "Gale",
          "text": "Amid the madness that has befallen us, it seems almost out of place to think of a kiss.",
          "context": "NodeContext: warm and affectionate",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
