    "3": {
      "id": "3",
      "speaker": "Gale",
      "text": "What did I think about seeing my head on a spike?",
      "context": "NodeContext: irked",
      "checkflags": [
        "CAMP_Gale_CRD_SpellTeaching_Pictured_Spike"
      ],
      "setflags": [],
