              "is_end": false,
              "node_type": "normal",
              "children": {
                "39": {
                  "id": "39",
                  "speaker": "Lae'zel",
                  "text": "You probably believe the illithid's twaddle about Vlaakith usurping Gith's throne as well.",
                  "context": "NodeContext: Pro-Vlaakith path, confident and only slightly vehement",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "55": {
                      "id": "55",
                      "speaker": "Lae'zel",
                      "text": "The Astral Prison contains not one, but two atrocities. They will use you and abuse you at every opportunity.",
                      "context": "NodeContext: Pro-Vlaakith path, confident and only slightly vehement",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "40": {
                          "id": "40",
                          "speaker": "Lae'zel",
                          "text": "There is one truth: Vlaakith's truth. Everything else is apostasy.",
                          "context": "",
                          "checkflags": [],
                          "setflags": [],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
                          "goto": "",
                          "link": "",
                          "is_end": false,
                          "node_type": "normal",
                          "children": {}
                        }
                      }
                    }
                  }
                }
              }
            }
          }
