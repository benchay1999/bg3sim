        "37": {
          "id": "37",
          "speaker": "Player",
          "text": "Why is he called the Prince of the Comet?",
          "context": "NodeContext: Pro-Orpheus path, confident but not vehement",
          "checkflags": [
            "ORI_Laezel_State_IsOnOrpheusPath",
