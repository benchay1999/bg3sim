    "6": {
      "id": "6",
      "speaker": "Nightwarden Minthara",
      "text": "Apologies if your heart raced more than it normally does after our last kiss. A trace of toxin clung to my lips.",
      "context": "",
      "checkflags": [
        "ORI_Minthara_Event_PoisonedKiss",
