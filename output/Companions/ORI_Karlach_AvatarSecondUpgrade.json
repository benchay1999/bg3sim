          "rolls": "",
          "goto": "",
          "link": "5",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "9": {
              "id": "9",
              "speaker": "Karlach",
              "text": "I've never considered it an option. The memories are too fresh.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "5",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "0": {
                  "id": "0",
                  "speaker": "Karlach",
                  "text": "I can't go back to Avernus. I can't let her get her claws in me again. Even if it means I... I have to...",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
