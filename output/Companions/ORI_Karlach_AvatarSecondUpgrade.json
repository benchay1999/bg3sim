                              "node_type": "normal",
                              "children": {
                                "18": {
                                  "id": "18",
                                  "speaker": "Karlach",
                                  "text": "That'll do me. My gods, that'd be enough.",
                                  "context": "",
                                  "checkflags": [],
                                  "setflags": [],
                                  "ruletags": [],
                                  "approval": [],
                                  "rolls": "",
                                  "goto": "",
