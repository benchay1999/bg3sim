                          "node_type": "normal",
                          "children": {
                            "13": {
                              "id": "13",
                              "speaker": "Karlach",
                              "text": "But the old bucket's got a bit of life in it yet. Enough to get home.",
                              "context": "",
                              "checkflags": [],
                              "setflags": [],
                              "ruletags": [],
                              "approval": [],
                              "rolls": "",
