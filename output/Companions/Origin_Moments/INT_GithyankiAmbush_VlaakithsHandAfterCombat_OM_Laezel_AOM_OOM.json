                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "31": {
                      "id": "31",
                      "speaker": "Lae'zel",
                      "text": "|I wish I could, but it's the only thing protecting me.|",
                      "context": "",
                      "checkflags": [],
