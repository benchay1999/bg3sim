          "children": {
            "30": {
              "id": "30",
              "speaker": "a4df3a1d-0c32-4548-acfe-5e45b1886671",
              "text": "|You have what Vlaakith is seeking.|",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
