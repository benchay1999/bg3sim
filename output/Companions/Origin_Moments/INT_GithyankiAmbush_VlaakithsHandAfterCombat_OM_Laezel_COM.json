                                          "children": {
                                            "20": {
                                              "id": "20",
                                              "speaker": "a4df3a1d-0c32-4548-acfe-5e45b1886671",
                                              "text": "|I am quite interested in seeing what you can do with it.|",
                                              "context": "",
                                              "checkflags": [],
                                              "setflags": [],
                                              "ruletags": [],
                                              "approval": [],
                                              "rolls": "",
                                              "goto": "",
                                              "link": "",
                                              "is_end": false,
                                              "node_type": "normal",
                                              "children": {
                                                "19": {
                                                  "id": "19",
                                                  "speaker": "a4df3a1d-0c32-4548-acfe-5e45b1886671",
                                                  "text": "|I will let you keep it for now, but will keep a close watch over your actions.|",
                                                  "context": "",
                                                  "checkflags": [],
                                                  "setflags": [],
                                                  "ruletags": [],
                                                  "approval": [],
