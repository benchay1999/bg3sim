            "3": {
              "id": "3",
              "speaker": "Narrator",
              "text": "*Your chest tightens as your exhausted father approaches.*",
              "context": "",
              "checkflags": [],
              "setflags": [
                "IRN_IronThrone_Event_RavengardLeftForCamp"
              ],
              "ruletags": [],
              "approval": [],
