  "dialogue": {
    "2": {
      "id": "2",
      "speaker": "Jhessem",
      "text": "A fine day to you, saer. Are you known to this court?",
      "context": "NodeContext: Swooping in to preserve the child's make-believe after the player ruined it.",
      "checkflags": [],
      "setflags": [
        "LOW_JaheirasHouse_Event_JaheiraSpoketoDaughter02"
