            "0": {
              "id": "0",
              "speaker": "Jord",
              "text": "I tended to it. I just let it... thrive in its own independence. You know, same way you raised us.",
              "context": "NodeContext: Well meant potshot at his mother, no malice in it",
              "checkflags": [],
              "setflags": [],
