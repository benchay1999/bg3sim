      "is_end": false,
      "node_type": "normal",
      "children": {
        "14": {
          "id": "14",
          "speaker": "Jaheira",
          "text": "There is nothing back there of worth to anyone but me.",
          "context": "",
          "checkflags": [],
          "setflags": [],
