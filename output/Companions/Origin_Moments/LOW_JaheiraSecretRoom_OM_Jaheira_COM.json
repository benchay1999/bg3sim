          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "13",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "4": {
              "id": "4",
              "speaker": "Player",
              "text": "I also hide all my worthless things behind secret doors.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "13",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "3": {
                  "id": "3",
                  "speaker": "Jaheira",
                  "text": "It should be no surprise to you, that Harpers like to hold their secrets close. Whether there is sense in it or not.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "13",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "11": {
                      "id": "11",
                      "speaker": "Jaheira",
                      "text": "On my word. All you will find inside is dust, and the mouldering keepsakes of a much younger woman.",
