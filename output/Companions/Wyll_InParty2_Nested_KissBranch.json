            "33": {
              "id": "33",
              "speaker": "Player",
              "text": "I wish I could kiss you, Wyll.",
              "context": "",
              "checkflags": [
                "GLO_ForgingOfTheHeart_State_KarlachSecondUpgrade = False",
                "REALLY_KARLACH"
              ],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "36": {
                  "id": "36",
                  "speaker": "Wyll",
                  "text": "I know. But until that day comes, I'm content just knowing you're near.",
                  "context": "",
                  "checkflags": [
                    "Wyll_InParty2_Event_KarlachAskedKiss"
                  ],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {}
                },
                "26": {
                  "id": "26",
                  "speaker": "Wyll",
                  "text": "I want nothing more. Unique as your heart might be, I still feel it tugging at mine.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [
                    "Wyll_InParty2_Event_KarlachAskedKiss"
                  ],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "3": {
                      "id": "3",
                      "speaker": "Wyll",
                      "text": "We'll have our chance. I'm sure of it.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {}
                    }
                  }
                }
              }
            },
            "37": {
              "id": "37",
              "speaker": "Player",
              "text": "What am I to you?",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "1": {
                  "id": "1",
                  "speaker": "Wyll",
                  "text": "A few moonrises back, I thought the answer was complicated. But it's quite simple, really:",
                  "context": "",
                  "checkflags": [
                    "CAMP_Wyll_Act3Romance_Event_Began"
                  ],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "25": {
                      "id": "25",
                      "speaker": "Wyll",
                      "text": "You are my yesterday, my today, and my tomorrow.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "13": {
                          "id": "13",
                          "speaker": "Wyll",
                          "text": "And one day, I might even call you consort to Grand Duke Wyll Ravengard of Baldur's Gate. My pride, my joy.",
                          "context": "",
                          "checkflags": [
                            "GLO_Wyll_State_GrandDuke",
                            "REALLY_KARLACH = False"
                          ],
                          "setflags": [],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
                          "goto": "",
                          "link": "",
                          "is_end": false,
                          "node_type": "normal",
                          "children": {}
                        },
                        "8": {
                          "id": "8",
                          "speaker": "Wyll",
                          "text": "Fortune willing, as the seasons of life pass into winter and my hair goes to grey, you'll still be within my quivering arm's reach.",
                          "context": "",
                          "checkflags": [
                            "REALLY_KARLACH = False"
                          ],
                          "setflags": [],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
                          "goto": "",
                          "link": "",
                          "is_end": false,
                          "node_type": "normal",
                          "children": {}
                        }
                      }
                    }
                  }
                },
                "19": {
                  "id": "19",
                  "speaker": "Wyll",
                  "text": "Ah. How to answer a question with so many layers as that?",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "30": {
                      "id": "30",
                      "speaker": "Wyll",
                      "text": "You are the red of the sunrise, the yellow of high noon, and the orange of sundown. If you were a song, I'd never stop singing; if you were a psalm, I'd never stop praying.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "29": {
                          "id": "29",
                          "speaker": "Wyll",
                          "text": "You make a damn fun dance partner, too.",
                          "context": "",
                          "checkflags": [],
                          "setflags": [],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
                          "goto": "",
                          "link": "",
                          "is_end": false,
                          "node_type": "normal",
                          "children": {}
                        }
                      }
                    }
                  }
                }
              }
            },
            "42": {
              "id": "42",
              "speaker": "Player",
              "text": "No more kisses, no more dances. I want to be allies, nothing more.",
              "context": "",
              "checkflags": [
                "ORI_State_PartneredWithWyll"
              ],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "9": {
                  "id": "9",
                  "speaker": "Wyll",
                  "text": "I - I don't know what to say.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "22": {
                      "id": "22",
                      "speaker": "Wyll",
                      "text": "I saw our future in my dreams: grand adventures, tender moments, and all the wonders in between them.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "43": {
                          "id": "43",
                          "speaker": "Wyll",
                          "text": "I'll let you go if I must, without crying or complaint - but with the gratitude that I could know the happiness of us.",
                          "context": "",
                          "checkflags": [],
                          "setflags": [],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
                          "goto": "",
                          "link": "",
                          "is_end": false,
                          "node_type": "normal",
                          "children": {
                            "16": {
                              "id": "16",
                              "speaker": "Player",
                              "text": "It's over, Wyll.",
                              "context": "",
                              "checkflags": [],
                              "setflags": [],
                              "ruletags": [],
                              "approval": [],
                              "rolls": "",
                              "goto": "",
                              "link": "",
                              "is_end": false,
                              "node_type": "normal",
                              "children": {
                                "11": {
                                  "id": "11",
                                  "speaker": "Wyll",
                                  "text": "So be it.",
                                  "context": "",
                                  "checkflags": [],
                                  "setflags": [],
                                  "ruletags": [],
                                  "approval": [],
                                  "rolls": "",
                                  "goto": "",
                                  "link": "",
                                  "is_end": false,
                                  "node_type": "normal",
                                  "children": {
                                    "23": {
                                      "id": "23",
                                      "speaker": "Wyll",
                                      "text": "Ah. You're the author of your own story. You get to choose which part I play.",
                                      "context": "",
                                      "checkflags": [],
                                      "setflags": [
                                        "ORI_State_WasPartneredWithWyll",
                                        "ORI_State_PartneredWithWyll = False",
                                        "ORI_State_DatingWithWyll = False"
                                      ],
                                      "ruletags": [],
                                      "approval": [],
                                      "rolls": "",
                                      "goto": "",
                                      "link": "",
                                      "is_end": false,
                                      "node_type": "normal",
                                      "children": {
                                        "32": {
                                          "id": "32",
                                          "speaker": "Wyll",
                                          "text": "Just do me a favour - make it a story worth telling, a life worth living. You deserve nothing less.",
                                          "context": "",
                                          "checkflags": [],
                                          "setflags": [
                                            "ORI_InParty_Event_EnterFromNestedEnd"
                                          ],
                                          "ruletags": [],
                                          "approval": [],
                                          "rolls": "",
                                          "goto": "",
                                          "link": "",
                                          "is_end": true,
                                          "node_type": "normal",
                                          "children": {}
                                        }
                                      }
                                    }
                                  }
                                }
                              }
                            },
                            "5": {
                              "id": "5",
                              "speaker": "Player",
                              "text": "I've changed my mind. I still want to be together.",
                              "context": "",
                              "checkflags": [],
                              "setflags": [],
                              "ruletags": [],
                              "approval": [],
                              "rolls": "",
                              "goto": "",
                              "link": "",
                              "is_end": false,
                              "node_type": "normal",
                              "children": {
                                "10": {
                                  "id": "10",
                                  "speaker": "Wyll",
                                  "text": "I'm glad. I want that too.",
                                  "context": "",
                                  "checkflags": [],
                                  "setflags": [],
                                  "ruletags": [],
                                  "approval": [],
                                  "rolls": "",
                                  "goto": "",
                                  "link": "",
                                  "is_end": false,
                                  "node_type": "normal",
                                  "children": {
                                    "6": {
                                      "id": "6",
                                      "speaker": "",
                                      "text": "",
                                      "context": "",
                                      "checkflags": [],
                                      "setflags": [],
                                      "ruletags": [],
                                      "approval": [],
                                      "rolls": "",
                                      "goto": "27",
                                      "link": "",
                                      "is_end": false,
                                      "node_type": "jump",
                                      "children": {}
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            },
