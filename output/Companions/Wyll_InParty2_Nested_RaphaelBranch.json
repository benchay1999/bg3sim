                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "38",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "24": {
                      "id": "24",
                      "speaker": "Wyll",
                      "text": "The farmer's dearest fruit, naturally, was no apple nor peach, but his beloved daughter.",
                      "context": "NodeContext: Wyll makes a few hoarse sounds at the end, as if he's losing his voice.",
                      "checkflags": [],
                      "setflags": [],
