                            "5": {
                              "id": "5",
                              "speaker": "Player",
                              "text": "With the right teacher, yes. What are your thoughts on the devil, Wyll?",
                              "context": "NodeContext: Wyll makes a few hoarse sounds at the end, as if he's losing his voice.",
                              "checkflags": [],
                              "setflags": [],
                              "ruletags": [],
