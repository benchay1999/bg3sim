                              "children": {
                                "16": {
                                  "id": "16",
                                  "speaker": "Wyll",
                                  "text": "Once all this is ended, once we've crushed the brain and freed the people -",
                                  "context": "",
                                  "checkflags": [],
                                  "setflags": [],
