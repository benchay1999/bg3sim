                                  "children": {
                                    "8": {
                                      "id": "8",
                                      "speaker": "Wyll",
                                      "text": "- I could once more call Baldur's Gate home. Whether I return there, well... only time will tell.",
                                      "context": "",
                                      "checkflags": [],
                                      "setflags": [],
                                      "ruletags": [],
                                      "approval": [],
