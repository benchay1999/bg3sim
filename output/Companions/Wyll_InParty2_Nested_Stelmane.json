                                                  "goto": "",
                                                  "link": "",
                                                  "is_end": false,
                                                  "node_type": "rollresult",
                                                  "children": {
                                                    "33": {
                                                      "id": "33",
                                                      "speaker": "Wyll",
                                                      "text": "Hm. If there is, I'm not aware of it.",
                                                      "context": "NodeContext: Slightly unsure",
                                                      "checkflags": [],
                                                      "setflags": [],
                                                      "ruletags": [],
                                                      "approval": [],
                                                      "rolls": "",
