                              "rolls": "",
                              "goto": "",
                              "link": "19",
                              "is_end": false,
                              "node_type": "normal",
                              "children": {}
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
