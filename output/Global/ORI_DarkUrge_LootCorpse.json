  "dialogue": {
    "11": {
      "id": "11",
      "speaker": "Narrator",
      "text": "*You drink in the pungent corpse.*",
      "context": "",
      "checkflags": [],
      "setflags": [],
      "ruletags": [],
      "approval": [],
      "rolls": "",
      "goto": "",
      "link": "0",
      "is_end": false,
      "node_type": "normal",
      "children": {
        "12": {
          "id": "12",
          "speaker": "Narrator",
          "text": "*Since you awoke on the ship, your mind has been cold and empty. But something stirs, with your hands close to this body.*",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "0",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "10": {
              "id": "10",
              "speaker": "Narrator",
              "text": "*You know nothing of why. But you find a half-smile flittering across your face.*",
              "context": "",
              "checkflags": [],
              "setflags": [
                "ORI_DarkUrge_Event_HadUrge"
              ],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "0",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "1": {
                  "id": "1",
                  "speaker": "Narrator",
                  "text": "Let the grin fall across your face.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "0",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "4": {
                      "id": "4",
                      "speaker": "Narrator",
                      "text": "*This feels like home.*",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "0": {
                          "id": "0",
                          "speaker": "Narrator",
                          "text": "*There is something unknown and unspeakable, deep within your heart.*",
                          "context": "",
                          "checkflags": [],
                          "setflags": [
                            "ORI_DarkUrge_LootedCorpse"
                          ],
                          "ruletags": [],
