                    }
                  }
                },
                "13": {
                  "id": "13",
                  "speaker": "Narrator",
                  "text": "Push the smile away. This is vile.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "0",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "7": {
                      "id": "7",
                      "speaker": "Narrator",
                      "text": "*With a shake of your head, you can hear your own blood throbbing in your ears.*",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "0",
