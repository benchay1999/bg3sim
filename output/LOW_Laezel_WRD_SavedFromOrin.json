    "9": {
      "id": "9",
      "speaker": "Lae'zel",
      "text": "It is not in a githyanki's nature to say 'thank you'.",
      "context": "NodeContext: KRAYTH kahn-ZHARN",
      "checkflags": [
        "ORI_Laezel_State_OrinAbductionDiscussionEnterFromQuestionBank"
      ],
      "setflags": [
        "ORI_Laezel_State_OrinAbductionDiscussionEnterFromQuestionBank = False",
