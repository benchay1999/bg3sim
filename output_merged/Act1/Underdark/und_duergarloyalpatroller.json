            "01_19": {
              "id": "01_19",
              "speaker": "Player",
              "text": "Tell me the drow put up a fight, at least.",
              "context": "",
              "checkflags": [
                "UND_DuergarLoyalPatroller_Event_AskedAboutDrow",
