      "AD_ArrivalStatuePlaque": {
        "synopsis": "A plaque at the base of a statue of Lathander.",
        "how_to_trigger": "Go to -47; 52; -134 in CRE_Main_A and click the plaque at the base of the lathander statue"
      },
      "AD_GraveMarker01": {
        "synopsis": "Warning signpost near SCL entrance",
        "how_to_trigger": "Click on the shortest grave marker on the left in X: -139 Y:56 Z: -132"
      },
      "AD_SCLSign": {
        "synopsis": "An old plaque made by the Lathander monks, placed near the entrance of SCL",
        "how_to_trigger": "Interact with the fallen plaque at CRE_Main_A X: -114 Y: 66 Z: -128"
      },
      "AD_FerrySign": {
        "synopsis": "A plaque guiding pilgrims to the ferry.",
        "how_to_trigger": "Read the plaque on the way to the ferry."
      },
      "AD_FerryResult": {
        "synopsis": "This happens when the player attempts the athletics check to move the monastery ferry.",
        "how_to_trigger": "Go to the ferry and attempt to pull the wheel."
      },
      "AD_GraveMarker02": {
        "synopsis": "Warning signpost near SCL entrance",
        "how_to_trigger": "Click on the shortest grave marker on the left in X: -139 Y:56 Z: -132"
      },
      "PAD_LathanderStatueRecognised": {
        "synopsis": "Recognising Lathander, the god of Dawn on a statue.",
        "how_to_trigger": "- Go to -47; 52; -134 in CRE_Main_A and click the plaque at the base of the lathander statue. - Wait of the AD to finish. - Succeed on the religion check."
      },
      "AD_GraveMarker03": {
        "synopsis": "Warning signpost near SCL entrance.",
        "how_to_trigger": "Click on the shortest grave markerin X: -132 Y:58 Z: -158"
      },
      "AD_Roadsign01": {
        "synopsis": "Roadsign with directions",
        "how_to_trigger": "Interact with the roadsign in CRE_Main_A at X: -72 Y: 66 Z:-137"
      },
      "PAD_MonasteryVista": {
        "synopsis": "The player characters take in the sight of the Monastery from the nearby cliff.",
        "how_to_trigger": "Walk into the vista trigger at -31; 57; -98."
      },
      "AD_Roadsign": {
        "synopsis": "Roadsigns Pointing towards moonrise towers and the Lathander Monastery",
        "how_to_trigger": "Click on S_CRE_Exterior_Roadsign_aa45043b-5cdd-439a-94e1-286842225277 at -50; 53; -111."
      },
      "AD_GraveMarker00": {
        "synopsis": "Warning signpost near entrance to SCL",
        "how_to_trigger": "Click on the rightmost grave marker in X: -139 Y:56 Z: -132"
      },
      "AD_Roadsign02": {
        "synopsis": "Roadsign with directions",
        "how_to_trigger": "Interact with the roadsign in CRE_Main_A at X: -10 Y: 22  Z: -15"
      },
