    "HumanLocal_2": {
      "id": "HumanLocal_2",
      "speaker": "Tylman Sands",
      "text": "Hold on, aren't you one of those adventurer ruffians the papers talk about? Bloody hells, better watch myself around you.",
      "context": "NodeContext: Wary",
      "checkflags": [
        "LOW_BaldursMouth_State_PositiveArticlePublished = False",
