    "HumanLocal_0": {
      "id": "HumanLocal_0",
      "speaker": "Tylman Sands",
      "text": "Baldur's Mouth was singing your praises, wasn't it? Not every day I meet someone famous.",
      "context": "NodeContext: Impressed",
      "checkflags": [
        "LOW_BaldursMouth_State_PositiveArticlePublished",
        "LOW_BaldursMouth_State_NegativeArticlePublished = False"
