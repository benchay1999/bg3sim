    "Supporter_01_4": {
      "id": "Supporter_01_4",
      "speaker": "Saer Manzarde",
      "text": "Tread carefully, outsider. I've got my eye on you - and I think the Steel Watch does as well.",
      "context": "",
      "checkflags": [
        "S_LOW_GeniusGortash_HasMet_Supporter_01",
        "RARE"
      ],
      "setflags": [],
      "ruletags": [
