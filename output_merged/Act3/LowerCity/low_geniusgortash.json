  "dialogue": {
    "Supporter_02_0": {
      "id": "Supporter_02_0",
      "speaker": "Frothilda",
      "text": "Just think, with how advanced the Steel Watch is now - imagine where we'll be in a year. In a decade! Exciting times indeed.",
      "context": "",
      "checkflags": [
        "S_LOW_GeniusGortash_HasMet_Supporter_02"
      ],
      "setflags": [],
