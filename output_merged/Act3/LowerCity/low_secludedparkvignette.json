          "children": {
            "AD_Character_01_Character_02_Dog_10": {
              "id": "AD_Character_01_Character_02_Dog_10",
              "speaker": "Phinn Truscott",
              "text": "We've kept going so far, despite... our loss. If you can keep on, I can keep on with you.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
