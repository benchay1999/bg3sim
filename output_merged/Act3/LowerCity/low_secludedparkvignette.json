      "children": {
        "AD_Character_01_Character_02_Dog_11": {
          "id": "AD_Character_01_Character_02_Dog_11",
          "speaker": "Phinn Truscott",
          "text": "I know. Every loss diminishes us further. And I'm afraid many more are on the way. What can we do?",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
