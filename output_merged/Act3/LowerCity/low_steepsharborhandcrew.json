    "AD_StrikeDiscussion_16": {
      "id": "AD_StrikeDiscussion_16",
      "speaker": "Tugelnhorn",
      "text": "It ain't so bad - could be a damn sight worse. You've got a job and a place to rest your head -",
      "context": "NodeContext: Trying to be reasonable (cut off at the end by friend interjecting)",
      "checkflags": [
        "16877fc1-3dd6-79d7-9daa-2f03cbcf7f69_B"
      ],
      "setflags": [
        "16877fc1-3dd6-79d7-9daa-2f03cbcf7f69_C"
      ],
      "ruletags": [],
      "approval": [],
      "rolls": "",
      "goto": "",
      "link": "",
      "is_end": false,
      "node_type": "normal",
      "children": {
        "AD_StrikeDiscussion_17": {
          "id": "AD_StrikeDiscussion_17",
          "speaker": "Ormont",
          "text": "- When they bloody let me rest my head.",
          "context": "NodeContext: Disgruntled, cutting in.",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "AD_StrikeDiscussion_3": {
              "id": "AD_StrikeDiscussion_3",
              "speaker": "Tugelnhorn",
              "text": "Well plenty don't even have that, have they? What about all those refugees? They've got nothing.",
              "context": "NodeContext: Trying to be reasonable",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "AD_StrikeDiscussion_15": {
                  "id": "AD_StrikeDiscussion_15",
                  "speaker": "Ormont",
                  "text": "What about them? You want them flooding in here, driving down our wages even more?",
                  "context": "NodeContext: Unswayed",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "AD_StrikeDiscussion_13": {
                      "id": "AD_StrikeDiscussion_13",
                      "speaker": "Tugelnhorn",
                      "text": "Well... no. Wouldn't want that.",
                      "context": "NodeContext: Hesitant",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": true,
                      "node_type": "normal",
                      "children": {}
                    }
                  }
                }
              }
            }
          }
        }
      }
