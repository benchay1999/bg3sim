      "goto": "",
      "link": "",
      "is_end": false,
      "node_type": "normal",
      "children": {
        "AD_Citizen_01_Citizen_02_13": {
          "id": "AD_Citizen_01_Citizen_02_13",
          "speaker": "Megthanet",
          "text": "I heard Gortash was strangled with a piece of his own intestine!",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
