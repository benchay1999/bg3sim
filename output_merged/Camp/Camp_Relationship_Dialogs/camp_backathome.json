              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "CRD_Karlach_25": {
                  "id": "CRD_Karlach_25",
                  "speaker": "Player",
                  "text": "I'm ready if you are.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
