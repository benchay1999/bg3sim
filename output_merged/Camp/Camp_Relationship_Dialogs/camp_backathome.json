                      "children": {
                        "CRD_Karlach_31": {
                          "id": "CRD_Karlach_31",
                          "speaker": "Karlach",
                          "text": "Can't wait to be home. Anything that comes after that is extra.",
                          "context": "",
                          "checkflags": [],
