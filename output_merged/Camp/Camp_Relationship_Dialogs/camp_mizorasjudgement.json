      "node_type": "normal",
      "children": {
        "CRD_Karlach_16": {
          "id": "CRD_Karlach_16",
          "speaker": "Karlach",
          "text": "Been a long time since someone stuck their neck out for me like that.",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
