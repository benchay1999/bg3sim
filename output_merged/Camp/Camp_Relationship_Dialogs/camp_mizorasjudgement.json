          "node_type": "normal",
          "children": {
            "CRD_Karlach_23": {
              "id": "CRD_Karlach_23",
              "speaker": "Player",
              "text": "He's a good man. Maybe the best of us.",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "3",
