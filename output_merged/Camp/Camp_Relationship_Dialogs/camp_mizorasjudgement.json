              "children": {
                "CRD_Karlach_4": {
                  "id": "CRD_Karlach_4",
                  "speaker": "Karlach",
                  "text": "Wouldn't be courage without the risk, would it.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
