      "OM_Wyll_AOM_OOM": {
        "synopsis": "Wyll avatar is killed once he passes the doors to the ketheric showdown.",
        "how_to_trigger": "- start with Wyll avatar- goto 804.34, -41.79, -26.41 and click on the door, open it- you get a warning you may die- proceed further in and skip the dialog- wyll just dies (this dialog)"
      },
      "OM_Wyll_COM": {
        "synopsis": "Wyll companion dies if the party proceeded through the door to Ketheric Showdown.",
        "how_to_trigger": "- Recruit Wyll as a companion- goto 804.34, -41.79, -26.41 and click on the door, open it- you get a warning you may die- proceed further in and skip the dialog- wyll just dies (this dialog)"
      },
      "": {
        "synopsis": "Wyll companion dies if the party proceeded through the door to Ketheric Showdown, but wasn't together with the party at the moment.",
        "how_to_trigger": "- Recruit Wyll as a companion, but send him to camp- goto 804.34, -41.79, -26.41 and click on the door, open it- you get a warning you may die- proceed further in and skip the dialog- wyll just dies (this dialog)"
      }
    },
