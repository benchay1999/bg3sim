              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "OM_Karlach_COM_3",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "OM_Karlach_COM_20": {
                  "id": "OM_Karlach_COM_20",
                  "speaker": "GROUP_SteelWatchers",
                  "text": "Ah. I understand your malfunction.",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
