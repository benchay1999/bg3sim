                              "checkflags": [],
                              "setflags": [],
                              "ruletags": [],
                              "approval": [],
                              "rolls": "",
                              "goto": "",
                              "link": "",
                              "is_end": false,
                              "node_type": "normal",
                              "children": {
                                "OM_Karlach_COM_11": {
                                  "id": "OM_Karlach_COM_11",
                                  "speaker": "Karlach",
                                  "text": "If it turns out I'm the fucking prototype for these things, I'll dismantle every single one of them.",
