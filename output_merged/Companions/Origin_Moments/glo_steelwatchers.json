                                  "setflags": [],
                                  "ruletags": [],
                                  "approval": [],
                                  "rolls": "",
                                  "goto": "",
                                  "link": "",
                                  "is_end": false,
                                  "node_type": "normal",
                                  "children": {
                                    "OM_Karlach_COM_16": {
                                      "id": "OM_Karlach_COM_16",
                                      "speaker": "GROUP_SteelWatchers",
                                      "text": "Any threat to the Steel Watch is a threat to Lord Gortash himself. Please report to the Steel Watch Foundry for immediate dismantling.",
