    "individual_metadata": {
      "ShovelRequired": {
        "synopsis": "Used by Digging system, when player presses on a revealed dirt mound to try to dig, but doesn't have a shovel.",
        "how_to_trigger": ""
      },
      "CannotUseNow": {
        "synopsis": "Character reactions when they try to use an item that cannot be currently used.",
        "how_to_trigger": ""
      },
      "ItemBlocked": {
        "synopsis": "Triggers whenever an item is blocked/stuck.",
        "how_to_trigger": ""
      },
      "ShovelFailed": {
        "synopsis": "Used by the digging system, when player uses shovel on the ground, but there are no diggable mounds in the vicinity.",
        "how_to_trigger": ""
      },
      "UnlockedElsewhere": {
        "synopsis": "Character reactions when they try to use an item that is locked and can be unlocked from somewhere else.",
        "how_to_trigger": ""
      },
      "ActivatedElsewhere": {
        "synopsis": "Character reactions when they try to use an item that is inactive and can be activated from somewhere else.",
        "how_to_trigger": ""
      },
      "CantTalkNow": {
        "synopsis": "|Fallback AD when an NPC is not available for dialog because they're too far from their scene, or involved in addressing a crime (or reacting to another disturbance -- anything from \"investigating\" to \"fleeing away in panic\"). Will never trigger for BEAST or MONSTER.|",
        "how_to_trigger": ""
      },
      "MechanicalClick": {
        "synopsis": "A mechanical trap goes *click*.An organic one goes *squish*.",
        "how_to_trigger": ""
      },
      "MechanicalClunk": {
        "synopsis": "The resisting \"klunk\" of stuck machinery.",
        "how_to_trigger": ""
      },
      "CannotUseNow_Muted": {
        "synopsis": "When the player clicks on an item that would start a dialog but the player is mute (silenced/wildshaped/..)",
        "how_to_trigger": ""
      },
      "CannotUseNow_Combat": {
        "synopsis": "Character reactions when they try to use an item that cannot be currently used because they are in combat.",
        "how_to_trigger": ""
      }
    },
