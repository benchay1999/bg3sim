{
  "metadata": {
    "individual_metadata": {
      "PAD_Blur": {
        "synopsis": "Each creature within 20 feet of the player becomes blurred for the next 30 seconds.",
        "how_to_trigger": ""
      },
      "PAD_Swap": {
        "synopsis": "For the next minute, all of the player's spells that target a creature swaps the player's position with the target's. Player casts spell, triggers wild magic, and then the NEXT spell they cast, they swap.",
        "how_to_trigger": "Trigger wild magic"
      },
      "PAD_Enchant": {
        "synopsis": "Each creature within 20 feet of the player has its weapon enchanted. Next attack using these weapons is guaranteed critical and does extra damage.",
        "how_to_trigger": "Trigger wild magic."
      },
      "PAD_TurnMagic": {
        "synopsis": "For the next 30 seconds, player triggers a wild magic surge at the start of every turn. Any outcome can be triggered except this one (so burning, fog etc)",
        "how_to_trigger": "Trigger wild magic"
      },
      "PAD_Burning": {
        "synopsis": "Each creature in a 20-foot-radius centered on the player is set ablaze by magical flame.",
        "how_to_trigger": "Trigger wild magic."
      },
      "PAD_Mephit": {
        "synopsis": "Player summoned a mephit that is hostile to everyone. Mud mephit is summoned until level 4. Player level 4 onwards summons Magma mephit",
        "how_to_trigger": "trigger wild magic"
      },
      "PAD_SorceryPoints": {
        "synopsis": "Until the end of the player's next turn, each spell the player casts restores sorcery points equal to its level of spell slots used.",
        "how_to_trigger": "trigger wild magic"
      },
      "PAD_Healing": {
        "synopsis": "For the next minute, all of the player's spells of 1st level or higher will cause a 10 feet explosion that heals all creatures within for 1d4 per level of spell slot.",
        "how_to_trigger": ""
      },
      "PAD_Fog": {
        "synopsis": "The player casts a fog cloud centered on itself.",
        "how_to_trigger": "trigger wild magic"
      },
      "PAD_Teleport": {
        "synopsis": "For the next minute, the player can teleport up to 20 feet as a bonus action on each turn.",
        "how_to_trigger": ""
      }
    },
