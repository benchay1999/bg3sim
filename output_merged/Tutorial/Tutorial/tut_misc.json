{
  "metadata": {
    "individual_metadata": {
      "PAD_OriginPod_Empty": {
        "synopsis": "One of the first ADs in the game. Player has crashlanded on the mind flayer ship and investigates other pods around them. These are empty - no one is inside. These lines will only appear when clicking on the pods of your fellow avatars (i.e. other people in multiplayer)",
        "how_to_trigger": ""
      },
      "MindflayerTransformation": {
        "synopsis": "The player sees a lever transform someone into a mind flayer. This is unusual and shouldn't happen. Gith guide will be shocked by what they see.",
        "how_to_trigger": ""
      },
      "AD_AltGuide_MindflayerHostile": {
        "synopsis": "Alt guide warns Lae'zel player not to attack.",
        "how_to_trigger": ""
      },
      "AD_AltGuide_Backtrack": {
        "synopsis": "ADs from alt guide in tutprial",
        "how_to_trigger": ""
      },
      "PAD_GenericNautiloidItem": {
        "synopsis": "Player touches a generic weird mind flayer item in the nautiloid",
        "how_to_trigger": ""
      },
      "PAD_BlockedElevatorLever": {
        "synopsis": "Player tries to use any elevator levers when it's moving or it is already in the lever's position.",
        "how_to_trigger": ""
      },
      "SpareLinesLaezel": {
        "synopsis": "Spare Lae'zel lines for Devora's last session.",
        "how_to_trigger": ""
      },
      "AD_LaezelMindflayerHostile": {
        "synopsis": "Triggered when the upper deck intellect devourer or the transformation room intellect devourer are attacked and go hostile. Also works with the upper deck dialogue hostility.",
        "how_to_trigger": ""
      },
      "PAD_RestingStation": {
        "synopsis": "Player interacts for the first time with a resting station at the nautiloid. This is especially so that players who have full HP will understand what the resting station does.",
        "how_to_trigger": ""
      },
      "AD_LaezelBacktrack": {
        "synopsis": "Lae'zel urges the player not to backtrack. She wants them to go forward to the helm, not mess around. Can be played throughout the tutorial.",
        "how_to_trigger": ""
      },
      "PAD_OriginPod_PlayerEmpty": {
        "synopsis": "Started when clicking on a pod that belongs to the avatar clicking on it.",
        "how_to_trigger": ""
      },
      "PAD_Aquarium": {
        "synopsis": "Player clicks on an 'aquarium' that has floating brains inside.",
        "how_to_trigger": "Touch aquarium objects in TUT_Avernus_C like the one in -5; 17; -411"
      },
      "PAD_NautiloidTank": {
        "synopsis": "Clicking on one of the mind flayer tanks. There are several around so we're providing plenty of variation on a theme.",
        "how_to_trigger": ""
      },
      "PAD_NonBrainAquarium": {
        "synopsis": "Player clicks on an 'aquarium' that has some alien plants and rocks inside.",
        "how_to_trigger": "Touch aquarium objects in TUT_Avernus_C like the one in -5; 17; -411"
      },
      "PAD_TransformationReaction": {
        "synopsis": "Player reaction to seeing the prisoner transform into a mind flayer after clicking on the first pod.",
        "how_to_trigger": ""
      },
      "PodMindflayer": {
        "synopsis": "A newly born mind flayer (still in its pod) stares at the player.",
        "how_to_trigger": ""
      }
    },
