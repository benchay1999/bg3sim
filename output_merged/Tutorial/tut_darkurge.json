      "children": {
        "Start_6": {
          "id": "Start_6",
          "speaker": "Narrator",
          "text": "*That pounding blood obscures who you are. An overwhelming loss of memory.*",
          "context": "",
          "checkflags": [],
          "setflags": [],
          "ruletags": [],
          "approval": [],
          "rolls": "",
          "goto": "",
          "link": "",
          "is_end": false,
          "node_type": "normal",
          "children": {
            "Start_0": {
              "id": "Start_0",
              "speaker": "Narrator",
              "text": "*How you ended up amidst these hellish flames is just as hidden.*",
              "context": "",
              "checkflags": [],
              "setflags": [],
              "ruletags": [],
              "approval": [],
              "rolls": "",
              "goto": "",
              "link": "",
              "is_end": false,
              "node_type": "normal",
              "children": {
                "Start_2": {
                  "id": "Start_2",
                  "speaker": "Narrator",
                  "text": "*You have nothing in your skull, besides your name and a headache. But you are in danger.*",
                  "context": "",
                  "checkflags": [],
                  "setflags": [],
                  "ruletags": [],
                  "approval": [],
                  "rolls": "",
                  "goto": "",
                  "link": "",
                  "is_end": false,
                  "node_type": "normal",
                  "children": {
                    "Start_7": {
                      "id": "Start_7",
                      "speaker": "GROUP_ORI_DU",
                      "text": "Curse whoever did this to you.",
                      "context": "",
                      "checkflags": [],
                      "setflags": [],
                      "ruletags": [],
                      "approval": [],
                      "rolls": "",
                      "goto": "",
                      "link": "",
                      "is_end": false,
                      "node_type": "normal",
                      "children": {
                        "Start_1": {
                          "id": "Start_1",
                          "speaker": "Narrator",
                          "text": "*The torturer behind this cruel prank will meet your malice when you get out of here.*",
                          "context": "",
                          "checkflags": [],
                          "setflags": [],
                          "ruletags": [],
                          "approval": [],
                          "rolls": "",
                          "goto": "",
